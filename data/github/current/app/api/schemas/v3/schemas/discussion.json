{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/discussion.json",
  "title": "Discussion",
  "type": "object",
  "definitions": {
    "body": {
      "type": "string",
      "example": "I want to talk about this."
    },
    "body_html": {
      "type": "string"
    },
    "body_text": {
      "type": "string"
    },
    "answer_html_url": {
      "type": [
        "string",
        "null"
      ],
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/discussions/1347#discussioncomment-9"
    },
    "answer_chosen_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2011-04-22T13:33:48Z"
    },
    "comments": {
      "type": "integer",
      "example": 0
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/issues/1347"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDU6SXNzdWUx"
    },
    "number": {
      "type": "integer",
      "example": 1347
    },
    "repository_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World"
    },
    "state": {
      "type": "string",
      "example": "open"
    },
    "locked": {
      "type": "boolean",
      "example": true
    },
    "active_lock_reason": {
      "type": [
        "string",
        "null"
      ],
      "example": "too heated"
    },
    "title": {
      "type": "string",
      "example": "Found a bug"
    },
    "author_association": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-22T13:33:48Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-22T13:33:48Z"
    },
    "timeline_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/timeline"
    }
  },
  "properties": {
    "body": {
      "$ref": "#/definitions/body"
    },
    "body_html": {
      "$ref": "#/definitions/body_html"
    },
    "body_text": {
      "$ref": "#/definitions/body_text"
    },
    "category": {
      "$ref": "https://schema.github.com/v3/discussion-category.json#"
    },
    "answer_html_url": {
      "$ref": "#/definitions/answer_html_url"
    },
    "answer_chosen_at": {
      "$ref": "#/definitions/answer_chosen_at"
    },
    "answer_chosen_by": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "comments": {
      "$ref": "#/definitions/comments"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "number": {
      "$ref": "#/definitions/number"
    },
    "repository_url": {
      "$ref": "#/definitions/repository_url"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "locked": {
      "$ref": "#/definitions/locked"
    },
    "active_lock_reason": {
      "$ref": "#/definitions/active_lock_reason"
    },
    "title": {
      "$ref": "#/definitions/title"
    },
    "user": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "repository": {
      "$ref": "https://schema.github.com/v3/repository.json#"
    },
    "author_association": {
      "$ref": "#/definitions/author_association"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "reactions": {
      "$ref": "https://schema.github.com/v3/reaction-rollup.json#"
    },
    "timeline_url": {
      "$ref": "#/definitions/timeline_url"
    }
  },
  "additionalProperties": false,
  "required": [
    "category",
    "comments",
    "html_url",
    "id",
    "node_id",
    "number",
    "repository_url",
    "state",
    "locked",
    "title",
    "user",
    "author_association",
    "created_at",
    "updated_at"
  ]
}
