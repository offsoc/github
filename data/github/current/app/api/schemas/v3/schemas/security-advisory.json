{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/security-advisory.json",
  "title": "SecurityAdvisory",
  "description": "A GitHub Security Advisory",
  "type": "object",
  "definitions": {
    "ghsa_id": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "severity": {
      "type": "string",
      "enum": [
        "low",
        "moderate",
        "high",
        "critical"
      ]
    },
    "identifiers": {
      "type": "array"
    },
    "references": {
      "type": "array"
    },
    "published_at": {
      "example": "2018-05-07T08:00:00-07:00",
      "format": "date-time",
      "type": "string"
    },
    "updated_at": {
      "example": "2018-05-07T08:00:00-07:00",
      "format": "date-time",
      "type": "string"
    },
    "withdrawn_at": {
      "example": "2018-05-07T08:00:00-07:00",
      "format": "date-time",
      "type": [
        "string",
        "null"
      ]
    },
    "vulnerabilities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "package": {
            "type": "object"
          },
          "severity": {
            "$ref": "#/definitions/severity"
          },
          "vulnerable_version_range": {
            "type": "string"
          },
          "first_patched_version": {
            "type": "object"
          }
        },
        "required": [
          "package",
          "severity",
          "vulnerable_version_range",
          "first_patched_version"
        ],
        "additionalProperties": false
      }
    },
    "cvss": {
      "type": "object",
      "properties": {
        "vector_string": {
          "type": [
            "string",
            "null"
          ]
        },
        "score": {
          "type": "number"
        }
      }
    },
    "cvss_severities": {
      "type": "object",
      "properties": {
        "cvss_v3": {
          "type": "object",
          "properties": {
            "vector_string": {
              "type": [
                "string",
                "null"
              ]
            },
            "score": {
              "type": "number"
            }
          }
        },
        "cvss_v4": {
          "type": "object",
          "properties": {
            "vector_string": {
              "type": [
                "string",
                "null"
              ]
            },
            "score": {
              "type": "number"
            }
          }
        }
      }
    },
    "cwes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "cwe_id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        }
      }
    }
  },
  "properties": {
    "ghsa_id": {
      "$ref": "#/definitions/ghsa_id"
    },
    "summary": {
      "$ref": "#/definitions/summary"
    },
    "description": {
      "$ref": "#/definitions/description"
    },
    "severity": {
      "$ref": "#/definitions/severity"
    },
    "identifiers": {
      "$ref": "#/definitions/identifiers"
    },
    "references": {
      "$ref": "#/definitions/references"
    },
    "published_at": {
      "$ref": "#/definitions/published_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "withdrawn_at": {
      "$ref": "#/definitions/withdrawn_at"
    },
    "vulnerabilities": {
      "$ref": "#/definitions/vulnerabilities"
    },
    "cvss": {
      "$ref": "#/definitions/cvss"
    },
    "cvss_severities": {
      "$ref": "#/definitions/cvss_severities"
    },
    "cwes": {
      "$ref": "#/definitions/cwes"
    }
  },
  "additionalProperties": false,
  "required": [
    "ghsa_id",
    "summary",
    "description",
    "severity",
    "identifiers",
    "references",
    "published_at",
    "updated_at",
    "withdrawn_at",
    "vulnerabilities"
  ]
}
