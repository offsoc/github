    },
    "statuses_url": {
      "type": "string",
      "example": "http://api.github.com/repos/octocat/Hello-World/statuses/{sha}"
    },
    "subscribers_url": {
      "type": "string",
      "format": "uri",
      "example": "http://api.github.com/repos/octocat/Hello-World/subscribers"
    },
    "subscription_url": {
      "type": "string",
      "format": "uri",
      "example": "http://api.github.com/repos/octocat/Hello-World/subscription"
    },
    "tags_url": {
      "type": "string",
      "format": "uri",
      "example": "http://api.github.com/repos/octocat/Hello-World/tags"
    },
    "teams_url": {
      "type": "string",
      "format": "uri",
      "example": "http://api.github.com/repos/octocat/Hello-World/teams"
    },
    "trees_url": {
      "type": "string",
      "example": "http://api.github.com/repos/octocat/Hello-World/git/trees{/sha}"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World"
    },
    "clone_url": {
      "type": "string",
      "example": "https://github.com/octocat/Hello-World.git"
    },
    "forks": {
      "type": "integer"
    },
    "forks_count": {
      "type": "integer",
      "example": 9
    },
    "git_url": {
      "type": "string",
      "example": "git:github.com/octocat/Hello-World.git"
    },
    "has_pages": {
      "type": "boolean"
    },
    "repository_homepage": {
      "type": [
        "string",
        "null"
      ],
      "format": "uri",
      "example": "https://github.com"
    },
    "language": {
      "type": [
        "string",
        "null"
      ]
    },
    "master_branch": {
      "type": "string"
    },
    "mirror_url": {
      "type": [
        "string",
        "null"
      ],
      "format": "uri",
      "example": "git:git.example.com/octocat/Hello-World"
    },
    "open_issues": {
      "type": "integer"
    },
    "open_issues_count": {
      "type": "integer",
      "example": 0
    },
    "temp_clone_token": {
      "type": "string"
    },
    "pushed_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2011-01-26T19:06:43Z"
    },
    "size": {
      "type": "integer",
      "example": 108
    },
    "ssh_url": {
      "type": "string",
      "example": "git@github.com:octocat/Hello-World.git"
    },
    "stargazers_count": {
      "type": "integer",
      "example": 80
    },
    "svn_url": {
      "type": "string",
      "format": "uri",
      "example": "https://svn.github.com/octocat/Hello-World"
    },
    "topics": {
      "type": "array",
