{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/pre-receive-environment-download.json",
  "title": "Pre-Receive Environment Download",
  "description": "The download status for a pre-receive environment.",
  "type": "object",
  "definitions": {
    "pre-receive-environment-download-subschema": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "url": {
          "$ref": "#/definitions/url"
        },
        "state": {
          "$ref": "#/definitions/state"
        },
        "downloaded_at": {
          "$ref": "#/definitions/downloaded_at"
        },
        "message": {
          "$ref": "#/definitions/message"
        }
      },
      "additionalProperties": false,
      "required": [
        "url",
        "state",
        "downloaded_at",
        "message"
      ]
    },
    "downloaded_at": {
      "description": "A timestamp, e.g. for created_at",
      "example": "2007-10-20T05:24:19Z",
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "state": {
      "description": "The status of the most recent attempt to download the environment image",
      "type": "string",
      "enum": [
        "not_started",
        "in_progress",
        "success",
        "failed"
      ],
      "example": "open"
    },
    "message": {
      "description": "Any messages about the most recent download attempt. If there were errors, they will be shown here.",
      "type": [
        "string",
        "null"
      ],
      "example": "Fix #42"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.example.com/api/v3/admin/pre-receive-environments/2/downloads/latest"
    }
  },
  "properties": {
    "url": {
      "$ref": "#/definitions/url"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "downloaded_at": {
      "$ref": "#/definitions/downloaded_at"
    },
    "message": {
      "$ref": "#/definitions/message"
    }
  },
  "additionalProperties": false,
  "required": [
    "url",
    "state",
    "downloaded_at",
    "message"
  ]
}
