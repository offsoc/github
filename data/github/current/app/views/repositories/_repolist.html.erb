<ul data-filterable-for="your-repos-filter" data-filterable-type="substring">
  <% is_registry_enabled = PackageRegistryHelper.show_packages? %>
  <% repos.each do |repository| %>
    <%= render(Repositories::ListItemComponent.new(
      repository: repository,
      is_registry_enabled: is_registry_enabled
    )) %>
  <% end %>
</ul>
