<% if total_licenses && !business.metered_plan %>
  <div>
    <span <%= test_selector "license-count-number" %>><%= number_with_delimiter(consumed_licenses) %></span><span class="color-fg-subtle"> / <span <%= test_selector "license-count-total" %>><%= number_with_delimiter(total_licenses) %></span></span>
  </div>
<% else %>
  <span <%= test_selector "license-count-number" %>>
    <%= number_with_delimiter(consumed_licenses) %>
  </span>
<% end %>
