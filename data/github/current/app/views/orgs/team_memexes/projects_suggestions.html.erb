<% if suggestions[:projects].size > 0 %>
  <%= render(Primer::Alpha::ActionList.new(
    select_variant: :multiple,
    show_dividers: true,
    p: 2,
    role: "listbox",
    aria: {
      label: "projects"
    }
  )) do |list| %>
    <% suggestions[:projects].each do |project_params| %>
      <% list.with_item(
        active: project_params[:selected],
        data: {
          name: project_params[:name],
          number: project_params[:number],
          owner: project_params[:owner]
        }
      ) do |item| %>
        <% item.with_description { project_params[:owner] } %>
        <% item.with_leading_visual_icon(icon: :table) %>
        <%= project_params[:name] %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
