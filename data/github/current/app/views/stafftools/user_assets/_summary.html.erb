<dl>
  <dt>ID</dt>
  <dd><%= asset.id %></dd>
</dl>
<dl>
  <dt>GUID</dt>
  <dd><%= asset.guid %></dd>
</dl>
<dl>
  <dt>Filename</dt>
  <dd><%= asset.name %></dd>
</dl>
<dl>
  <dt>Uploader</dt>
  <dd>
    <% if asset.uploader %>
      <%= link_to asset.uploader, stafftools_user_path(asset.uploader) %>
    <% else %>
      <%= asset.user_id %>
    <% end %>
  </dd>
</dl>
<dl>
  <dt>Created</dt>
  <dd><%= asset.created_at %></dd>
</dl>
