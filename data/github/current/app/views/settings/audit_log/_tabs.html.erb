<% if GitHub.audit_log_export_enabled? && GitHub.flipper[:audit_log_export_logs].enabled?(current_user) %>
<nav class="UnderlineNav mb-4" aria-label="Audit Log">
  <div class="UnderlineNav-body" role="tablist">
      <%= selected_link_to "Events",
        settings_user_audit_log_path,
        class: "UnderlineNav-item",
        data: test_selector_hash("underline-nav-item"),
        role: "tab" %>
      <%= selected_link_to "Export logs",
        settings_user_audit_log_export_logs_path,
        class: "UnderlineNav-item",
        data: test_selector_hash("underline-nav-item"),
        role: "tab",
        highlight: :settings_user_audit_log_export_logs %>
  </div>
</nav>
<% end %>
