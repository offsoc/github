{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/pre-receive-hook-error.json",
  "title": "Pre-Receive Hook Error",
  "description": "A pre-receive hook error",
  "type": "object",
  "definitions": {
    "hook_warning": {
      "description": "Warning message describing the pre-receive hook failure",
      "type": "string"
    },
    "hook_error": {
      "description": "Error message describing the pre-receive hook failure",
      "type": "string"
    },
    "real_ip": {
      "description": "IP address of the pusher",
      "type": "string"
    },
    "pre_receive_hook_id": {
      "description": "Unique identifier of the pre-receive hook",
      "example": 11235813,
      "type": "integer"
    }
  },
  "links": [
    {
      "href": "/internal/pre-receive-hooks/{pre_receive_hook_id}/errors",
      "method": "POST",
      "rel": "create",
      "schema": {
        "properties": {
          "pusher": {
            "$ref": "https://schema.github.com/v3/git-push.json#/definitions/pusher"
          },
          "ref_updates": {
            "$ref": "https://schema.github.com/v3/git-push.json#/definitions/ref_updates"
          },
          "protocol": {
            "$ref": "https://schema.github.com/v3/git-push.json#/definitions/protocol"
          },
          "hook_warning": {
            "$ref": "#/definitions/hook_warning"
          },
          "hook_error": {
            "$ref": "#/definitions/hook_error"
          },
          "real_ip": {
            "$ref": "#/definitions/real_ip"
          }
        },
        "required": [
          "pusher",
          "ref_updates",
          "protocol",
          "hook_warning",
          "hook_error",
          "real_ip"
        ],
        "type": "object",
        "title": "Create"
      },
      "title": "Create a new pre-receive hook error"
    }
  ]
}
