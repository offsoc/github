<% if GitHub.actions_enabled? && GitHub.marketplace_enabled? %>
  <% if action_release.present? && action_release.published_on_marketplace? %>
    <%= render Primer::Beta::Label.new(scheme: :secondary, size: :large, vertical_align: :text_bottom, test_selector: "marketplace-label", **system_arguments) do %>
      <%= link_to "Marketplace", marketplace_action_path(action_release.repository_action.slug), data: hydro_data %>
    <% end %>
  <% end %>
<% end %>
