<% if view.entry.hit.data[:two_factor_type] %>
  <% if view.entry.hit.data[:two_factor_type] == "github_mobile" %>
    Two-factor challenge failed with GitHub Mobile <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "otp" %>
    Two-factor challenge failed with one time password <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "app" %>
    Two-factor challenge failed with authenticator app <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "sms" %>
    Two-factor challenge failed with SMS <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "recovery code" %>
    Two-factor challenge failed with recovery code <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "u2f" %>
    Two-factor challenge failed with security key <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "passkey" %>
    Two-factor challenge failed with passkey <%= view.entry.hit.data[:note].try(:downcase) %>
  <% end %>
<% else %>
  Two-factor challenge failure <%= view.entry.hit.data[:note].try(:downcase) %>
<% end %>
