{
  "app/api/*.rb": { "alternate": "test/integration/api/{}_test.rb" },
  "app/api/internal/twirp/**/*.rb": { "alternate": "test/twirp/{dirname}/{basename}_test.rb" },
  "app/api/serializer/*_dependency.rb": { "alternate": "test/models/api/serializer/{}_dependency_test.rb" },
  "app/assets/modules/**/*.tsx": { "alternate": "app/assets/modules/{dirname}/__tests__/{basename}.test.tsx" },
  "app/controllers/*.rb": { "alternate": "test/integration/{}_test.rb" },
  "app/*.rb": { "alternate": "test/{}_test.rb" },
  "app/components/*.html.erb": { "alternate": "test/components/{}_test.rb" },
  "app/view_models/*.rb": { "alternate": "test/view_models/{}_test.rb" },
  "lib/*.rb": { "alternate": "test/lib/{}_test.rb" },
  "lib/github/transitions/*.rb": { "alternate": "test/lib/github/transitions/{}_test.rb" },
  "packages/admin/app/*.rb": { "alternate": "packages/admin/test/{}_test.rb" },
  "packages/apps/app/*.rb": { "alternate": "packages/apps/test/{}_test.rb" },
  "packages/app_security/app/*.rb": { "alternate": "packages/app_security/test/{}_test.rb" },
  "packages/billing/app/*.rb": { "alternate": "packages/billing/test/{}_test.rb" },
  "packages/copilot/app/*.rb": { "alternate": "packages/copilot/test/{}_test.rb" },
  "packages/discussions/app/*.rb": { "alternate": "packages/discussions/test/{}_test.rb" },
  "packages/github_sponsors/app/*.rb": { "alternate": "packages/github_sponsors/test/{}_test.rb" },
  "packages/marketplace/app/*.rb": { "alternate": "packages/marketplace/test/{}_test.rb" },
  "packages/issues/app/*.rb": { "alternate": "packages/issues/test/{}_test.rb" },
  "packages/notifications/app/*.rb": { "alternate": "packages/notifications/test/{}_test.rb" },
  "packages/repositories/app/models/*.rb": { "alternate": "packages/repositories/test/{}_test.rb" },
  "packages/orgs/app/*.rb": { "alternate": "packages/orgs/test/{}_test.rb" },
  "packages/users/app/*.rb": { "alternate": "packages/users/test/{}_test.rb" },
  "packages/import_export/app/models/*.rb": { "alternate": "packages/import_export/test/models/{}_test.rb" },
  "packages/import_export/test/models/*_test.rb": { "alternate": "packages/import_export/app/models/{}.rb" },
  "packages/**/app/**/*.rb": {"alternate": "packages/{dirname}/test/{dirname}/{basename}_test.rb"},
  "packages/**/app/controllers/**/*.rb": {"alternate": "packages/{dirname}/test/integration/{dirname}/{basename}_test.rb"},
  "script/seeds/*.rb": { "alternate": "test/script/seeds/{}_test.rb" },
  "test/lib/github/transitions/*_test.rb": { "alternate": "lib/github/transitions/{}.rb" },
  "test/rubocop/cop/github/*.rb": { "alternate": "test/rubocop/tests/github/{}_test.rb" },
  "test/test_helpers/*.rb": { "alternate": "test/test_helper_tests/{}_test.rb" },
  "ui/packages/**/*.tsx": { "alternate": "ui/packages/{dirname}/__tests__/{basename}.test.tsx" },
  "ui/packages/**/src/*.tsx": { "alternate": "ui/packages/{dirname}/__tests__/{basename}.test.tsx" },
  "ui/packages/**/*.ts": { "alternate": "ui/packages/{dirname}/__browser-tests__/{basename}.test.ts" }
}
