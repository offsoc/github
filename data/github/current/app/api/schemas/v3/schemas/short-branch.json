{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/short-branch.json",
  "title": "Short Branch",
  "type": "object",
  "definitions": {
    "name": {
      "type": "string",
      "example": "master"
    }
  },
  "properties": {
    "name": {
      "$ref": "#/definitions/name"
    },
    "commit": {
      "type": "object",
      "properties": {
        "sha": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "additionalProperties": false,
      "required": [
        "sha",
        "url"
      ]
    },
    "protected": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "commit",
    "protected"
  ]
}
