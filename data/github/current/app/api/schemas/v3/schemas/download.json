{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/download.json",
  "title": "Download",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/downloads/1"
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/repos/octocat/Hello-World/downloads/new_file.jpg"
    },
    "name": {
      "type": "string",
      "example": "new_file.jpg"
    },
    "description": {
      "type": "string",
      "example": "Description of your download"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "size": {
      "type": "integer",
      "example": 1024
    },
    "download_count": {
      "type": "integer",
      "example": 40
    },
    "content_type": {
      "type": [
        "string",
        "null"
      ],
      "example": ".jpg"
    }
  },
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "description": {
      "$ref": "#/definitions/description"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "size": {
      "$ref": "#/definitions/size"
    },
    "download_count": {
      "$ref": "#/definitions/download_count"
    },
    "content_type": {
      "$ref": "#/definitions/content_type"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "url",
    "html_url",
    "name",
    "description",
    "created_at",
    "size",
    "download_count",
    "content_type"
  ]
}
