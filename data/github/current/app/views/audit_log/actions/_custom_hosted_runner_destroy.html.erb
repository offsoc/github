<% if view.org? %>
  <p><span class="context"><%= view.link_to_org %></span> Custom-hosted runner with id <%= view.entry.hit.data[:pool_id] %> deleted.</p>
<% elsif view.business? %>
  <p><span class="context"><%= view.link_to_business %></span> Custom-hosted runner with id <%= view.entry.hit.data[:pool_id] %> deleted.</p>
<% else %>
  <p>Custom-hosted runner with id <%= view.entry.hit.data[:pool_id] %> deleted.</p>
<% end %>


