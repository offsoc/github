{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/repository-identifier.json",
  "title": "Repository Identifier",
  "description": "A slimmed down repository payload",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "https://schema.github.com/v3/repository.json#/definitions/id"
    },
    "node_id": {
      "$ref": "https://schema.github.com/v3/repository.json#/definitions/node_id"
    },
    "name": {
      "$ref": "https://schema.github.com/v3/repository.json#/definitions/name"
    },
    "full_name": {
      "$ref": "https://schema.github.com/v3/repository.json#/definitions/full_name"
    },
    "private": {
      "$ref": "https://schema.github.com/v3/repository.json#/definitions/private"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "name",
    "full_name",
    "private"
  ]
}
