{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/project-column-move.json",
  "title": "Project Column Move",
  "description": "A project column move represents a change in position in the project.",
  "type": "object",
  "definitions": {
    "position": {
      "description": "The position of the column in a project",
      "example": "last",
      "type": "string",
      "pattern": "^(?:first|last|after:\\d+)$"
    },
    "column_id": {
      "description": "The unique identifier of the column the card should be moved to",
      "example": 42,
      "type": "integer"
    }
  },
  "links": [
    {
      "href": "/projects/columns/{column_id}/moves",
      "method": "POST",
      "rel": "create-legacy",
      "schema": {
        "properties": {
          "position": {
            "$ref": "#/definitions/position"
          }
        },
        "required": [
          "position"
        ],
        "type": "object"
      },
      "title": "Create"
    },
    {
      "title": "Create a new project column move",
      "method": "POST",
      "href": "/projects/columns/{column_id}/moves",
      "rel": "create",
      "schema": {
        "properties": {
          "position": {
            "$ref": "#/definitions/position"
          }
        },
        "required": [
          "position"
        ],
        "additionalProperties": false,
        "type": "object"
      }
    }
  ]
}
