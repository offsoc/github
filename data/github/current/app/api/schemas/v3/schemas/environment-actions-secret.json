{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/environment-actions-secret.json",
  "title": "Environment Actions Secret",
  "description": "Set secrets for a GitHub Actions environment.",
  "type": "object",
  "definitions": {
  },
  "links": [
    {
      "title": "Sets a key/value for an Actions environment secret.",
      "href": "/repositories/{repository_id}/environments/{environment}/secrets/{name}",
      "method": "PUT",
      "rel": "set-environment-secret",
      "schema": {
        "properties": {
          "key_id": {
            "$ref": "https://schema.github.com/v3/actions-secret.json#/definitions/key_id"
          },
          "encrypted_value": {
            "$ref": "https://schema.github.com/v3/actions-secret.json#/definitions/encrypted_value"
          }
        },
        "required": [
          "key_id",
          "encrypted_value"
        ],
        "type": "object"
      }
    },
    {
      "title": "Deletes a secret.",
      "href": "/repositories/{repository_id}/environments/{environment}/secrets/{name}",
      "method": "DELETE",
      "rel": "delete-environment-secret",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    }
  ],
  "properties": {
    "name": {
      "$ref": "https://schema.github.com/v3/actions-secret.json#/definitions/name"
    },
    "created_at": {
      "$ref": "https://schema.github.com/v3/actions-secret.json#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "https://schema.github.com/v3/actions-secret.json#/definitions/updated_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "created_at",
    "updated_at"
  ]
}
