<%# rubocop:disable Lint/InstanceVariable %>
<%# `view` is an instance of Orgs::Teams::TeamsPageView %>
<%
  page_info \
    title: "#{this_team.name} Team · Edit · General",
    dashboard_pinnable_item_id: this_team.global_relay_id
  %>

<%= render layout: "orgs/teams/team_settings_sidebar", locals: { org_login: @organization.display_login, team_slug: this_team.slug, selected: "General" } do %>
  <%=
    render "orgs/teams/form", view: create_view_model(
      Orgs::Teams::FormView,
      organization: @organization,
      team: this_team,
      team_params: defined?(team_params) ? team_params : {},
      team_entity: team_entity,
    )
  %>
<% end %>
