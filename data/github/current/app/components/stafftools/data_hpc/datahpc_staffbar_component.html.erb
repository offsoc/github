<datahpc-staffbar data-catalyst-inline hidden>
<%= render(Primer::Beta::Link.new(href: "https://github.com/github/web-performance-catalog-scorecard/blob/main/docs/web-performance.md#hpc", target: "_blank", id: "datahpc-staffbar-icon")) do |c| %>
  <% c.with_tooltip(text: "The page doesn't have a data-hpc element, so render time cannot be monitored. Click to show more details.") %>
  <%= render(Primer::Beta::Octicon.new(
      icon: :"alert-fill",
      size: :small,
      color: :on_emphasis,
      aria: { label: "data-hpc warning" }
    )) %>
<% end %>
</datahpc-staffbar>
