{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/feed.json",
  "title": "Feed",
  "type": "object",
  "definitions": {
    "timeline_url": {
      "type": "string",
      "example": "https://github.com/timeline"
    },
    "user_url": {
      "type": "string",
      "example": "https://github.com/{user}"
    },
    "security_advisories_url": {
      "type": "string",
      "example": "https://github.com/security-advisories"
    },
    "current_user_url": {
      "type": "string",
      "example": "https://github.com/octocat.private?token=abc123"
    },
    "current_user_public_url": {
      "type": "string",
      "example": "https://github.com/octocat"
    },
    "current_user_actor_url": {
      "type": "string",
      "example": "https://github.com/octocat.private.actor?token=abc123"
    },
    "current_user_organization_url": {
      "type": "string",
      "example": ""
    },
    "current_user_organization_urls": {
      "type": "array",
      "example": [
        "https://github.com/organizations/github/octocat.private.atom?token=abc123"
      ]
    }
  },
  "properties": {
    "_links": {
      "type": "object",
      "properties": {
        "timeline": {
          "$ref": "https://schema.github.com/v3/link-with-type.json#"
        },
        "user": {
          "$ref": "https://schema.github.com/v3/link-with-type.json#"
        },
        "security_advisories": {
          "$ref": "https://schema.github.com/v3/link-with-type.json#"
        },
        "current_user": {
          "$ref": "https://schema.github.com/v3/link-with-type.json#"
        },
        "current_user_public": {
          "$ref": "https://schema.github.com/v3/link-with-type.json#"
        },
        "current_user_actor": {
          "$ref": "https://schema.github.com/v3/link-with-type.json#"
        },
        "current_user_organization": {
          "$ref": "https://schema.github.com/v3/link-with-type.json#"
        },
        "current_user_organizations": {
          "type": "array",
          "items": {
            "$ref": "https://schema.github.com/v3/link-with-type.json#"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "timeline",
        "user"
      ]
    },
    "timeline_url": {
      "$ref": "#/definitions/timeline_url"
    },
    "user_url": {
      "$ref": "#/definitions/user_url"
    },
    "security_advisories_url": {
      "$ref": "#/definitions/security_advisories_url"
    },
    "current_user_url": {
      "$ref": "#/definitions/current_user_url"
    },
    "current_user_public_url": {
      "$ref": "#/definitions/current_user_public_url"
    },
    "current_user_actor_url": {
      "$ref": "#/definitions/current_user_actor_url"
    },
    "current_user_organization_url": {
      "$ref": "#/definitions/current_user_organization_url"
    },
    "current_user_organization_urls": {
      "$ref": "#/definitions/current_user_organization_urls"
    }
  },
  "additionalProperties": false,
  "required": [
    "_links",
    "timeline_url",
    "user_url"
  ]
}
