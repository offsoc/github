{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/clone-traffic.json",
  "title": "Clone Traffic",
  "type": "object",
  "definitions": {
    "uniques": {
      "type": "integer",
      "example": 128
    },
    "count": {
      "type": "integer",
      "example": 173
    },
    "clones": {
      "type": "array",
      "items": {
        "$ref": "https://schema.github.com/v3/traffic.json#"
      }
    }
  },
  "properties": {
    "uniques": {
      "$ref": "#/definitions/uniques"
    },
    "count": {
      "$ref": "#/definitions/count"
    },
    "clones": {
      "$ref": "#/definitions/clones"
    }
  },
  "additionalProperties": false,
  "required": [
    "uniques",
    "count",
    "clones"
  ]
}
