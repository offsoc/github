{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/pre-receive-environment.json",
  "title": "Pre-Receive Environment",
  "description": "A pre-receive environment is the environment in which a pre-receive-hook is executed.",
  "type": "object",
  "definitions": {
    "pre-receive-environment-subschema": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "id": {
          "$ref": "#/definitions/id"
        },
        "name": {
          "$ref": "#/definitions/name"
        },
        "image_url": {
          "$ref": "#/definitions/image_url"
        },
        "url": {
          "$ref": "#/definitions/url"
        },
        "html_url": {
          "$ref": "#/definitions/html_url"
        },
        "default_environment": {
          "$ref": "#/definitions/default_environment"
        },
        "created_at": {
          "$ref": "#/definitions/created_at"
        },
        "hooks_count": {
          "$ref": "#/definitions/hooks_count"
        },
        "download": {
          "$ref": "https://schema.github.com/v3/pre-receive-environment-download.json#/definitions/pre-receive-environment-download-subschema"
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "name",
        "image_url",
        "url",
        "html_url",
        "default_environment",
        "created_at",
        "hooks_count",
        "download"
      ]
    },
    "created_at": {
      "description": "A timestamp, e.g. for created_at",
      "example": "2007-10-20T05:24:19Z",
      "type": "string",
      "format": "date-time"
    },
    "downloaded_at": {
      "description": "A timestamp, e.g. for created_at",
      "example": "2007-10-20T05:24:19Z",
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "id": {
      "description": "Unique identifier of the pre-receive environment",
      "example": 42,
      "type": "integer"
    },
    "name": {
      "description": "The name of the pre-receive environment",
      "example": "Team Environment",
      "type": "string"
    },
    "image_url": {
      "description": "The url of the image used as a base for this environment.",
      "example": "https://example.com/path/to/my_environment.tar.gz",
      "type": "string"
    },
    "default_environment": {
      "description": "Whether this is the default pre-receive environment",
      "type": "boolean"
    },
    "url": {
      "type": "string",
      "example": "https://github.example.com/api/v3/admin/pre-receive-environments/2"
    },
    "html_url": {
      "description": "The html url of the pre-receive environment.",
      "example": "https://github.example.com/admin/pre-receive-environments/42",
      "type": "string"
    },
    "hooks_count": {
      "description": "The number of hooks configured to use this envionment",
      "type": "integer"
    },
    "pre_receive_environment_id": {
      "description": "Unique identifier of the pre-receive environment",
      "example": 87654321,
      "type": "integer"
    }
  },
  "links": [
    {
      "href": "/admin/pre-receive-environments",
      "method": "POST",
      "rel": "create",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "image_url": {
            "$ref": "#/definitions/image_url"
          }
        },
        "required": [
