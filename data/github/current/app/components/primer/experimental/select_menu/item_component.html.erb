<%= render wrapper_component do %>
  <% if icon_el = icon_component %>
    <%= render icon_el %>
  <% end %>
  <%= content %>
<% end %>

<% if divider.is_a?(String) %>
  <%= render divider_component do %>
    <%= divider %>
  <% end %>
<% elsif divider %>
  <%= render divider_component %>
<% end %>
