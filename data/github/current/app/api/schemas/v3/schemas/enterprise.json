{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/enterprise.json",
  "title": "Enterprise",
  "description": "An enterprise account",
  "type": "object",
  "definitions": {
    "id": {
      "description": "Unique identifier of the enterprise",
      "example": 42,
      "type": "integer"
    },
    "name": {
      "description": "The name of the enterprise.",
      "type": "string",
      "example": "Octo Business"
    },
    "slug": {
      "description": "The slug url identifier for the enterprise.",
      "type": "string",
      "example": "octo-business"
    },
    "avatar_url": {
      "type": "string",
      "format": "uri"
    },
    "description": {
      "description": "A short description of the enterprise.",
      "type": [
        "string",
        "null"
      ]
    },
    "website_url": {
      "description": "The enterprise's website URL.",
      "type": [
        "string",
        "null"
      ],
      "format": "uri"
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/enterprises/octo-business"
    },
    "node_id": {
      "type": "string",
      "example": "MDEwOlJlcG9zaXRvcnkxMjk2MjY5"
    },
    "created_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2019-01-26T19:01:12Z"
    },
    "updated_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2019-01-26T19:14:43Z"
    },
    "enterprise": {
      "type": "object",
      "properties": {
        "description": {
          "$ref": "#/definitions/description"
        },
        "html_url": {
          "$ref": "#/definitions/html_url"
        },
        "website_url": {
          "$ref": "#/definitions/website_url"
        },
        "id": {
          "$ref": "#/definitions/id"
        },
        "node_id": {
          "$ref": "#/definitions/node_id"
        },
        "name": {
          "$ref": "#/definitions/name"
        },
        "slug": {
          "$ref": "#/definitions/slug"
        },
        "created_at": {
          "$ref": "#/definitions/created_at"
        },
        "updated_at": {
          "$ref": "#/definitions/updated_at"
        },
        "avatar_url": {
          "$ref": "#/definitions/avatar_url"
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "node_id",
        "name",
        "slug",
        "html_url",
        "created_at",
        "updated_at",
        "avatar_url"
      ]
    }
  },
  "properties": {
    "description": {
      "$ref": "#/definitions/description"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "website_url": {
      "$ref": "#/definitions/website_url"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "slug": {
      "$ref": "#/definitions/slug"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "avatar_url": {
      "$ref": "#/definitions/avatar_url"
    }
  },
  "additionalProperties": false,
