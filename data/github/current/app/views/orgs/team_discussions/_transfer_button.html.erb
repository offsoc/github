<%= render(Primer::Alpha::Dialog.new(
  title: "Transfer team posts to discussions",
  classes: "Overlay--size-auto",
)) do |d| %>
  <% d.with_show_button(data: analytics_click_attributes(category: "team discussions", action: "transfer modal opened")) { "Transfer" } %>
  <% d.with_body(p: 0) do %>
    <%= render partial: "orgs/team_discussions/deprecation_dialog", locals: { organization: organization, team_slug: team_slug } %>
  <% end %>
<% end %>
