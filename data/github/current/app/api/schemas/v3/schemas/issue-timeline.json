{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/issue-timeline.json",
  "title": "Issue Timeline",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDEwOklzc3VlRXZlbnQx"
    },
    "url": {
      "type": "string",
      "example": "https://api.github.com/repos/octocat/Hello-World/issues/events/1"
    },
    "event": {
      "type": "string",
      "example": "closed"
    },
    "commit_id": {
      "type": [
        "string",
        "null"
      ],
      "example": "6dcb09b5b57875f334f61aebed695e2e4193db5e"
    },
    "commit_url": {
      "type": [
        "string",
        "null"
      ],
      "example": "https://api.github.com/repos/octocat/Hello-World/commits/6dcb09b5b57875f334f61aebed695e2e4193db5e"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-14T16:00:49Z"
    },
    "author_association": {
      "type": "string"
    },
    "lock_reason": {
      "type": [
        "string",
        "null"
      ]
    },
    "body": {
      "type": [
        "string",
        "null"
      ]
    },
    "html_url": {
      "type": "string"
    },
    "issue_url": {
      "type": "string"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "submitted_at": {
      "type": "string",
      "format": "date-time"
    },
    "pull_request_url": {
      "type": [
        "string",
        "null"
      ]
    },
    "message": {
      "type": "string"
    },
    "parents": {
      "type": "array"
    },
    "sha": {
      "type": "string"
    },
    "path": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "state_reason": {
      "description": "Reason for the current state",
      "example": "not_planned",
      "type": [
        "string",
        "null"
      ]
    },
    "diff_position": {
      "type": "integer"
    },
    "comments": {
      "type": "array"
    }
  },
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "actor": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "event": {
      "$ref": "#/definitions/event"
    },
    "commit_id": {
      "$ref": "#/definitions/commit_id"
    },
    "commit_url": {
      "$ref": "#/definitions/commit_url"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "label": {
      "$ref": "https://schema.github.com/v3/issue-event-label.json#"
    },
    "assignee": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "assigner": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "review_requester": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "requested_reviewer": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "requested_team": {
      "$ref": "https://schema.github.com/v3/team-simple.json#"
    },
    "dismissed_review": {
      "$ref": "https://schema.github.com/v3/issue-event-dismissed-review.json#"
    },
    "milestone": {
      "$ref": "https://schema.github.com/v3/issue-event-milestone.json#"
    },
    "project_card": {
      "$ref": "https://schema.github.com/v3/issue-event-project-card.json#"
    },
    "rename": {
      "$ref": "https://schema.github.com/v3/issue-event-rename.json#"
    },
    "author_association": {
      "$ref": "#/definitions/author_association"
    },
    "lock_reason": {
      "$ref": "#/definitions/lock_reason"
    },
    "issue": {
      "$ref": "https://schema.github.com/v3/issue.json#"
    },
    "body": {
      "$ref": "#/definitions/body"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "issue_url": {
      "$ref": "#/definitions/issue_url"
    },
    "user": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "source": {
      "$ref": "https://schema.github.com/v3/cross-reference-source.json#"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "submitted_at": {
      "$ref": "#/definitions/submitted_at"
    },
    "author": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/git-user.json#"
        }
      ]
    },
    "committer": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/git-user.json#"
        }
      ]
    },
    "pull_request_url": {
      "$ref": "#/definitions/pull_request_url"
    },
    "message": {
      "$ref": "#/definitions/message"
    },
    "parents": {
      "$ref": "#/definitions/parents"
    },
    "verification": {
      "$ref": "https://schema.github.com/v3/verification.json#"
    },
    "tree": {
      "type": "object",
      "properties": {
        "sha": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "additionalProperties": false,
      "required": [
        "sha",
        "url"
      ]
    },
    "sha": {
      "$ref": "#/definitions/sha"
    },
    "path": {
      "$ref": "#/definitions/path"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "state_reason": {
      "$ref": "#/definitions/state_reason"
    },
    "diff_position": {
      "$ref": "#/definitions/diff_position"
    },
    "performed_via_github_app": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/integration.json#"
        }
      ]
    },
    "reactions": {
      "$ref": "https://schema.github.com/v3/reaction-rollup.json#"
    },
    "comments": {
      "$ref": "#/definitions/comments"
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "$ref": "https://schema.github.com/v3/link.json#"
        },
        "html": {
          "$ref": "https://schema.github.com/v3/link.json#"
        },
        "pull_request": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "required": [
            "href"
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "html",
        "pull_request"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "event"
  ]
}
