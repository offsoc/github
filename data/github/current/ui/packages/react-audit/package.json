{
  "name": "@github-ui/react-audit",
  "description": "Tools for understanding React usage within this repo",
  "main": "react-audit.ts",
  "dev": true,
  "exports": {
    ".": "./react-audit.ts",
    "./utils": "./utils.ts"
  },
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 . --ext .js,.ts,.tsx,.json",
    "react-audit": "node -r @swc-node/register react-audit.ts",
    "react-component-audit": "node -r @swc-node/register react-component-audit.ts",
    "test": "jest",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@github-ui/eslintrc": "*",
    "@github-ui/jest": "*",
    "@swc-node/register": "1.9.1",
    "js-yaml": "3.13.1",
    "ts-morph": "18.0.0",
    "typescript": "*"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{workspaceRoot}/ui/packages/**/*"
      ]
    }
  },
  "private": true
