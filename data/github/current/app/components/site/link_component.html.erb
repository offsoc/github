<% cache_if @cache, cache_key do %>
  <%= content_tag(:a,
    class: link_classes,
    href: @url,
    data: @data,
    **@options
  ) do %>
    <%= GitHub::Goomba::SimplePipeline.to_html(@text, cache_settings: { use_cache: true }) %> <%= svg("modules/site/icons/chevrow.svg") if @arrow %>
  <% end %>
<% end %>
