{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/project-column.json",
  "title": "Project Column",
  "description": "Project columns contain cards of work.",
  "type": "object",
  "definitions": {
    "id": {
      "description": "The unique identifier of the project column",
      "example": 42,
      "type": "integer"
    },
    "column_id": {
      "description": "The ID of the column.",
      "type": "integer",
      "example": 42
    },
    "name": {
      "description": "Name of the project column",
      "example": "Remaining tasks",
      "type": "string"
    },
    "project_id": {
      "description": "The ID of the project.",
      "type": "integer"
    },
    "node_id": {
      "type": "string",
      "example": "MDEzOlByb2plY3RDb2x1bW4zNjc="
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/projects/columns/367"
    },
    "project_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/projects/120"
    },
    "cards_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/projects/columns/367/cards"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2016-09-05T14:18:44Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2016-09-05T14:22:28Z"
    }
  },
  "links": [
    {
      "href": "/projects/columns/{column_id}",
      "method": "PATCH",
      "rel": "update-legacy",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      },
      "title": "Update an existing project column"
    },
    {
      "href": "/projects/{project_id}/columns",
      "method": "POST",
      "rel": "create-legacy",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      },
      "title": "Create"
    },
    {
      "title": "Create a project column",
      "method": "POST",
      "href": "/projects/{project_id}/columns",
      "rel": "create",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          }
        },
        "required": [
          "name"
        ],
        "type": "object",
        "additionalProperties": false
      }
    },
    {
      "title": "Update an existing project column",
      "method": "PATCH",
      "href": "/projects/columns/{column_id}",
      "rel": "update",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          }
        },
        "required": [
          "name"
        ],
        "type": "object",
        "additionalProperties": false
      }
    },
    {
      "title": "Delete a project column",
      "method": "DELETE",
      "rel": "delete",
      "href": "/projects/columns/{column_id}",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "project_url": {
      "$ref": "#/definitions/project_url"
    },
    "cards_url": {
      "$ref": "#/definitions/cards_url"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "url",
    "project_url",
    "cards_url",
    "name",
    "created_at",
    "updated_at"
  ]
}
