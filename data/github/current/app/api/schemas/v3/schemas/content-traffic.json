{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/content-traffic.json",
  "title": "Content Traffic",
  "type": "object",
  "definitions": {
    "path": {
      "type": "string",
      "example": "/github/hubot"
    },
    "title": {
      "type": "string",
      "example": "github/hubot: A customizable life embetterment robot."
    },
    "uniques": {
      "type": "integer",
      "example": 2225
    },
    "count": {
      "type": "integer",
      "example": 3542
    }
  },
  "properties": {
    "path": {
      "$ref": "#/definitions/path"
    },
    "title": {
      "$ref": "#/definitions/title"
    },
    "uniques": {
      "$ref": "#/definitions/uniques"
    },
    "count": {
      "$ref": "#/definitions/count"
    }
  },
  "additionalProperties": false,
  "required": [
    "path",
    "title",
    "uniques",
    "count"
  ]
}
