{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/org-pre-receive-hook.json",
  "description": "A pre-receive hook that is enabled or testing on an organization",
  "title": "Organization pre-receive hook",
  "type": "object",
  "definitions": {
    "id": {
      "description": "Unique identifier of the pre-receive hook",
      "example": 42,
      "type": "integer"
    },
    "name": {
      "description": "The name of the pre-receive hook",
      "example": "Commit message enforcement",
      "type": "string"
    },
    "enforcement": {
      "description": "The state of enforcement for the pre-receive hook on this organization",
      "type": "string",
      "enum": [
        "enabled",
        "disabled",
        "testing"
      ]
    },
    "configuration_url": {
      "description": "The configuration URL for the pre-receive hook on the organization",
      "type": "string",
      "format": "uri"
    },
    "allow_downstream_configuration": {
      "description": "Whether repositories can override enforcement",
      "type": "boolean"
    }
  },
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "enforcement": {
      "type": "string"
    },
    "configuration_url": {
      "type": "string"
    },
    "allow_downstream_configuration": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "enforcement",
    "configuration_url",
    "allow_downstream_configuration"
  ]
}
