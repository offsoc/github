{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/workflow.json",
  "title": "Workflow",
  "description": "A GitHub Actions workflow",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 5
    },
    "inputs": {
      "description": "Inputs for the workflow_dispatch event",
      "example": {
        "name": "monalisa",
        "color": "green"
      },
      "type": "object",
      "maxProperties": 10,
      "additionalProperties": true
    },
    "ref": {
      "description": "The ref to trigger, this can be a branch, tag, or sha",
      "example": "topic-branch",
      "type": "string"
    },
    "repository_id": {
      "description": "The ID of the repository.",
      "type": "integer",
      "example": 42
    },
    "node_id": {
      "type": "string",
      "example": "MDg6V29ya2Zsb3cxMg=="
    },
    "name": {
      "type": "string",
      "example": "CI"
    },
    "path": {
      "type": "string",
      "example": "ruby.yml"
    },
    "state": {
      "type": "string",
      "example": "active",
      "enum": [
        "active",
        "deleted",
        "disabled_fork",
        "disabled_inactivity",
        "disabled_manually"
      ]
    },
    "url": {
      "type": "string",
      "example": "https://api.github.com/repos/actions/setup-ruby/workflows/5"
    },
    "html_url": {
      "type": "string",
      "example": "https://github.com/actions/setup-ruby/blob/master/.github/workflows/ruby.yml"
    },
    "badge_url": {
      "type": "string",
      "example": "https://github.com/actions/setup-ruby/workflows/CI/badge.svg"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2019-12-06T14:20:20.000Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2019-12-06T14:20:20.000Z"
    },
    "deleted_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2019-08-17T12:37:15Z"
    },
    "workflow_id": {
      "type": [
        "integer",
        "string"
      ],
      "description": "The ID of the workflow.",
      "example": [
        "5",
        ".github/workflows/ruby.yml"
      ]
    }
  },
  "links": [
    {
      "title": "Triggers a `workflow_dispatch` event",
      "href": "/repositories/{repository_id}/actions/workflows/{workflow_id}/dispatches",
      "method": "POST",
      "rel": "dispatch",
      "schema": {
        "properties": {
          "ref": {
            "$ref": "#/definitions/ref"
          },
          "inputs": {
            "$ref": "#/definitions/inputs"
          }
        },
        "additionalProperties": false,
        "required": [
          "ref"
        ],
        "type": "object"
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "path": {
      "$ref": "#/definitions/path"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "badge_url": {
      "$ref": "#/definitions/badge_url"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/created_at"
    },
    "deleted_at": {
      "$ref": "#/definitions/created_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "name",
    "path",
    "state",
    "url",
    "html_url",
    "badge_url",
    "created_at",
    "updated_at"
  ]
}
