<%# view is Oauth::AuthorizeView %>
<%# rubocop:disable GitHub/RailsViewRenderLiteral %>
<% view.requested_scope_families.each do |family| %>
  <% if view.generically_renderable_family?(family) %>
    <%= render partial: "oauth_accesses/scopes/generic_template", locals: {view: view, family: family} %>
  <% else %>
    <%= render partial: "oauth_accesses/scopes/#{family}", locals: {view: view, family: family} %>
  <% end %>
<% end %>
