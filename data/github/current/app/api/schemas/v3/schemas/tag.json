{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/tag.json",
  "title": "Tag",
  "type": "object",
  "definitions": {
    "name": {
      "type": "string",
      "example": "v0.1"
    },
    "node_id": {
      "type": "string"
    },
    "zipball_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/zipball/v0.1"
    },
    "tarball_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/tarball/v0.1"
    }
  },
  "properties": {
    "name": {
      "$ref": "#/definitions/name"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "commit": {
      "type": "object",
      "properties": {
        "sha": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "additionalProperties": false,
      "required": [
        "sha",
        "url"
      ]
    },
    "zipball_url": {
      "$ref": "#/definitions/zipball_url"
    },
    "tarball_url": {
      "$ref": "#/definitions/tarball_url"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "node_id",
    "commit",
    "zipball_url",
    "tarball_url"
  ]
}
