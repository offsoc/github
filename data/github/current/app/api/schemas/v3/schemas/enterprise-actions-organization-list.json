{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/enterprise-actions-organization-list.json",
  "title": "List of organization allowed to run Actions under this enterprise",
  "type": "object",
  "definitions": {
    "selected_organization_ids": {
      "description": "List of organization IDs for which to enable Actions.",
      "type": "array",
      "items": {
        "type": "integer"
      },
      "example": [
        42,
        1337
      ]
    },
    "total_count": {
      "description": "Total number of organizations that are allowed to run Actions, for pagination.",
      "type": "integer"
    },
    "organizations": {
      "description": "The URL of the selected actions if 'allowed_actions' is 'selected'.",
      "type": "array",
      "items": {
        "$ref": "https://schema.github.com/v3/organization-simple.json#"
      }
    }
  },
  "properties": {
    "organizations": {
      "$ref": "#/definitions/organizations"
    },
    "total_count": {
      "$ref": "#/definitions/total_count"
    }
  },
  "additionalProperties": false,
  "required": [
    "total_count",
    "organizations"
  ],
  "links": [
    {
      "title": "Enable GitHub Actions for a list of organizations",
      "href": "/enterprises/{enterprise_id}/actions/permissions/organizations",
      "method": "PUT",
      "rel": "update-organizations",
      "schema": {
        "properties": {
          "selected_organization_ids": {
            "$ref": "#/definitions/selected_organization_ids"
          }
        },
        "required": [
          "selected_organization_ids"
        ],
        "additionalProperties": false,
        "type": "object"
      }
    }
  ]
}
