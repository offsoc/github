<%= render Primer::Alpha::ActionMenu.new(
  menu_id: "attempt-navigation",
  src: menu_src_path,
  preload: true,
  test_selector: "attempt-navigation",
  mr: 2,
  anchor_align: :end,
  ) do |menu|
    menu.with_show_button do |button|
      button.with_leading_visual_icon(icon: :history, color: :muted)
      button.with_trailing_action_icon(icon: :"triangle-down", color: :muted)
      button_text
    end
  end
%>
