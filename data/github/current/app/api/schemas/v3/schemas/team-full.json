{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/team-full.json",
  "title": "Full Team",
  "description": "Groups of organization members that gives permissions on specified repositories.",
  "type": "object",
  "definitions": {
    "id": {
      "description": "Unique identifier of the team",
      "example": 42,
      "type": "integer"
    },
    "team_id": {
      "description": "The ID of the team.",
      "type": "integer"
    },
    "name": {
      "description": "Name of the team",
      "example": "Developers",
      "type": "string"
    },
    "description": {
      "description": "Description of the team",
      "example": "Developers! Developers! Developers!",
      "type": "string"
    },
    "permission": {
      "description": "Permission that the team will have for its repositories",
      "example": "push",
      "type": "string"
    },
    "privacy": {
      "description": "The level of privacy this team should have",
      "type": "string",
      "enum": [
        "closed",
        "secret"
      ],
      "example": "closed"
    },
    "notification_setting": {
      "description": "The possible team notification values.",
      "type": "string",
      "enum": [
        "notifications_enabled",
        "notifications_disabled"
      ],
      "example": "notifications_enabled"
    },
    "ldap_dn": {
      "description": "Distinguished Name (DN) that team maps to within LDAP environment",
      "example": "uid=example,ou=users,dc=github,dc=com",
      "type": "string"
    },
    "url": {
      "description": "URL for the team",
      "example": "https://api.github.com/organizations/1/team/1",
      "type": "string",
      "format": "uri"
    },
    "legacy_description": {
      "description": "Description of the team",
      "example": "Developers! Developers! Developers!",
      "type": [
        "string",
        "null"
      ]
    },
    "parent_team_id": {
      "description": "The ID of the parent team",
      "example": 38,
      "type": "number"
    },
    "maintainers": {
      "description": "Logins of users to add as maintainers to the team",
      "example": [
        "jakeboxer"
      ],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "repo_names": {
      "description": "Repositories to add to the team",
      "example": [
        "github/dotfiles"
      ],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "organization_id": {
      "description": "The ID of the organization.",
      "type": "integer"
    },
    "node_id": {
      "type": "string",
      "example": "MDQ6VGVhbTE="
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/orgs/rails/teams/core"
    },
    "members_url": {
      "type": "string",
      "example": "https://api.github.com/organizations/1/team/1/members{/member}"
    },
    "team_description": {
      "type": [
        "string",
        "null"
      ],
      "example": "A great team."
    },
    "repositories_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/organizations/1/team/1/repos"
    },
    "slug": {
      "type": "string",
      "example": "justice-league"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2017-07-14T16:53:42Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2017-08-17T12:37:15Z"
    },
    "members_count": {
      "type": "integer",
      "example": 3
    },
    "repos_count": {
      "type": "integer",
      "example": 10
    }
  },
  "links": [
    {
      "href": "/organizations/{organization_id}/teams",
      "method": "POST",
      "rel": "create-legacy",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "description": {
            "$ref": "#/definitions/legacy_description"
          },
          "repo_names": {
            "description": "Repositories to add to the team",
            "example": [
              "github/dotfiles"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "maintainers": {
            "description": "Logins of users to add as maintainers to the team",
            "example": [
              "jakeboxer"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "permission": {
            "$ref": "#/definitions/permission"
          },
          "ldap_dn": {
            "$ref": "#/definitions/ldap_dn"
          },
          "parent_team_id": {
            "description": "The ID of the parent team",
            "example": 38,
            "type": "number"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      },
      "title": "Create a new team"
    },
    {
      "href": "/organizations/{org_id}/team/{team_id}",
      "method": "PATCH",
      "rel": "update-legacy",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "description": {
            "$ref": "#/definitions/legacy_description"
          },
          "permission": {
            "$ref": "#/definitions/permission"
          },
          "parent_team_id": {
            "description": "The ID of the parent team",
            "example": 38,
            "type": [
              "number",
              "null"
            ]
          }
        },
        "type": [
          "object",
          "null"
        ]
      },
      "title": "Update a team"
    },
    {
      "href": "/organizations/{org_id}/team/{team_id}",
      "method": "POST",
      "rel": "update",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "description": {
            "$ref": "#/definitions/description"
          },
          "permission": {
            "$ref": "#/definitions/permission"
          },
          "parent_team_id": {
            "description": "The ID of the parent team",
            "example": 38,
            "type": [
              "number",
              "null"
            ]
          }
        },
        "type": [
          "object",
          "null"
        ]
      },
      "title": "Update a team"
    },
    {
      "title": "Create a new team",
      "method": "POST",
      "href": "/organizations/{organization_id}/teams",
      "rel": "create",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "description": {
            "$ref": "#/definitions/description"
          },
          "repo_names": {
            "$ref": "#/definitions/repo_names"
          },
          "maintainers": {
            "$ref": "#/definitions/maintainers"
          },
          "privacy": {
            "$ref": "#/definitions/privacy"
          },
          "notification_setting": {
            "$ref": "#/definitions/notification_setting"
          },
          "permission": {
            "$ref": "#/definitions/permission"
          },
          "ldap_dn": {
            "$ref": "#/definitions/ldap_dn"
          },
          "parent_team_id": {
            "$ref": "#/definitions/parent_team_id"
          }
        },
        "required": [
          "name"
        ],
        "type": "object",
        "additionalProperties": false
      }
    },
    {
      "title": "Update a team",
      "method": "PATCH",
      "href": "/organizations/{org_id}/team/{team_id}",
      "rel": "update",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "description": {
            "$ref": "#/definitions/description"
          },
          "permission": {
            "$ref": "#/definitions/permission"
          },
          "parent_team_id": {
            "description": "The ID of the parent team",
            "example": 38,
            "type": [
              "number",
              "null"
            ]
          }
        },
        "type": [
          "object",
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Delete team",
      "method": "DELETE",
      "rel": "delete",
      "href": "/organizations/{org_id}/team/{team_id}",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "members_url": {
      "$ref": "#/definitions/members_url"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "description": {
      "$ref": "#/definitions/team_description"
    },
    "permission": {
      "$ref": "#/definitions/permission"
    },
    "privacy": {
      "$ref": "#/definitions/privacy"
    },
    "notification_setting": {
      "$ref": "#/definitions/notification_setting"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "repositories_url": {
      "$ref": "#/definitions/repositories_url"
    },
    "slug": {
      "$ref": "#/definitions/slug"
    },
    "ldap_dn": {
      "$ref": "#/definitions/ldap_dn"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "members_count": {
      "$ref": "#/definitions/members_count"
    },
    "repos_count": {
      "$ref": "#/definitions/repos_count"
    },
    "organization": {
      "$ref": "https://schema.github.com/v3/organization.json#"
    },
    "parent": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/team-simple.json#"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "url",
    "members_url",
    "name",
    "description",
    "permission",
    "html_url",
    "repositories_url",
    "slug",
    "created_at",
    "updated_at",
    "members_count",
    "repos_count",
    "organization"
  ]
}
