<%= render layout: "sponsors/dashboards/layout", locals: { sponsors_listing: sponsors_listing, selected_tab: :settings } do %>
  <%= render Primer::Beta::Subhead.new(display: :flex, align_items: :center, flex: :auto) do |component| %>
    <% component.with_heading(tag: :h2) do %>Settings<% end %>
  <% end %>

  <%= render Sponsors::Dashboard::Settings::ManualPayoutsNoticeComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::LegalNameComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::ContactEmailComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::EmailSettingsComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::ConnectPatreonComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::DisconnectPatreonComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::PatreonEnablementComponent.new(
    sponsorable: sponsorable,
    location: Sponsors::PatreonEnablementComponent::Location::SponsorableDashboard,
  ) %>
  <%= render Sponsors::NoPublishedPatreonTierWarningComponent.new(
    sponsorable: sponsorable,
    location: Sponsors::NoPublishedPatreonTierWarningComponent::Location::SponsorableSettings,
  ) %>
  <%= render Sponsors::Dashboard::Tiers::PatreonTierDoesNotMeetMinimumNoticeComponent.new(
    sponsors_listing: sponsors_listing,
    location: Sponsors::Dashboard::Tiers::PatreonTierDoesNotMeetMinimumNoticeComponent::Location::SettingsPage,
  ) %>
  <%= render Sponsors::Dashboard::Settings::StripeAccountsComponent.new(
    sponsors_listing: sponsors_listing,
    stripe_accounts: stripe_connect_accounts,
    disable_stripe_links: disable_stripe_links
  ) %>
  <%= render Sponsors::Dashboard::Settings::StripePayoutsEmailComponent.new(
    sponsorable: sponsorable,
    disable_stripe_links: disable_stripe_links
  ) %>
  <%= render Sponsors::Dashboard::Settings::CountryOfResidenceComponent.new(
    sponsorable: sponsorable,
    return_to: params[:return_to],
    disable_stripe_links: disable_stripe_links
  ) %>
  <%= render Sponsors::Dashboard::Settings::BillingCountryComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::FiscalHostComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::OrganizationOwnersListComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::RedraftComponent.new(sponsorable: sponsorable) %>
  <%= render Sponsors::Dashboard::Settings::DisableComponent.new(sponsorable: sponsorable) %>
<% end %>
