<%# rubocop:disable GitHub/RailsViewRenderLiteral %>
<% Primer::Beta::NavList::Group.new.tap do |group| %>
  <% teams.each do |team| %>
    <%= render(
      group.build_avatar_item(
        src: team.primary_avatar_url,
        username: team.combined_slug,
        href: team_path(team),
        test_selector: "side-panel-item"
      )
    ) %>
  <% end %>
<% end %>
