<% hits.each_with_index do |hit, index| %>
    <%= render Primer::Beta::AutoComplete::Item.new(
      value: hit[:url],
      border: :bottom,
      selected: query.empty? || query == "#" ? false : index == 0,
      "data-name": hit[:id],
    ) do |c| %>

    <%= render TrackingBlocks::TasklistBlockAutocompleteResultComponent.new(hit: hit) %>
  <% end %>
<% end %>
