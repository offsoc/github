{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/package-version.json",
  "title": "Package Version",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "name": {
      "type": "string",
      "example": "latest"
    },
    "url": {
      "type": "string",
      "example": "https://api.github.com/orgs/github/packages/container/super-linter/versions/786068"
    },
    "package_html_url": {
      "type": "string",
      "example": "https://github.com/orgs/github/packages/container/package/super-linter"
    },
    "html_url": {
      "type": "string",
      "example": "https://github.com/orgs/github/packages/container/super-linter/786068"
    },
    "license": {
      "type": "string",
      "example": "MIT"
    },
    "description": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-10T20:09:31Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2014-03-03T18:58:10Z"
    },
    "deleted_at": {
      "type": "string",
      "format": "date-time",
      "example": "2014-03-03T18:58:10Z"
    },
    "metadata": {
      "type": "object",
      "title": "Package Version Metadata",
      "properties": {
        "package_type": {
          "type": "string",
          "example": "docker",
          "enum": [
            "npm",
            "maven",
            "rubygems",
            "docker",
            "nuget",
            "container"
          ]
        },
        "container": {
          "type": "object",
          "title": "Container Metadata",
          "properties": {
            "tags": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": [
            "tags"
          ]
        },
        "docker": {
          "type": "object",
          "title": "Docker Metadata",
          "properties": {
            "tags": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": [
            "tags"
          ]
        }
      },
      "required": [
        "package_type"
      ]
    }
  },
  "links": [
    {
      "title": "Delete Current User Package Version",
      "method": "DELETE",
      "href": "/user/packages/{package_type}/{package}/versions/{version_id}",
      "rel": "delete-current-user",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Delete User Package Version",
      "method": "DELETE",
      "href": "/user/{user_id}/packages/{package_type}/{package}/versions/{version_id}",
      "rel": "delete-user",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Delete Organization Package Version",
      "method": "DELETE",
      "href": "/organizations/{organization_id}/packages/{package_type}/{package}/versions/{version_id}",
      "rel": "delete-org",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Restore Current User Package Version",
      "method": "POST",
      "href": "/user/packages/{package_type}/{package}/versions/{version_id}/restore",
      "rel": "restore-current-user",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Restore User Package Version",
      "method": "POST",
      "href": "/user/{user_id}/packages/{package_type}/{package}/versions/{version_id}/restore",
      "rel": "restore-user",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Restore Organization Package Version",
      "method": "POST",
      "href": "/organizations/{organization_id}/packages/{package_type}/{package}/versions/{version_id}/restore",
      "rel": "restore-org",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "package_html_url": {
      "$ref": "#/definitions/package_html_url"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "license": {
      "$ref": "#/definitions/license"
    },
    "description": {
      "$ref": "#/definitions/description"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/created_at"
    },
    "deleted_at": {
      "$ref": "#/definitions/deleted_at"
    },
    "metadata": {
      "$ref": "#/definitions/metadata"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "url",
    "package_html_url",
    "created_at",
    "updated_at"
  ]
}
