{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/enterprise-installation-user-accounts-upload.json",
  "title": "Enterprise Server installation user accounts upload",
  "description": "An upload of Enterprise Server installation user accounts data",
  "type": "object",
  "definitions": {
    "id": {
      "description": "Unique identifier of the upload.",
      "example": 100,
      "type": "integer"
    },
    "name": {
      "description": "The file name of the upload.",
      "type": "string",
      "example": "ghe-io-201903281746.json"
    },
    "content_type": {
      "description": "The content type of the uploaded file.",
      "type": "string",
      "example": "application/json"
    },
    "state": {
      "description": "State of the upload.",
      "type": "string",
      "enum": [
        "starter",
        "uploaded"
      ],
      "example": "uploaded"
    },
    "size": {
      "description": "The size of the uploaded file in bytes.",
      "type": "integer",
      "example": 12345
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "description": "The date and time at which the upload was created",
      "example": "2019-01-26T19:14:43Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "description": "The date and time at which the upload was last updated",
      "example": "2019-01-26T19:14:43Z"
    },
    "sync_state": {
      "description": "The synchronization state of the uploaded user accounts data.",
      "type": "string",
      "enum": [
        "pending",
        "success",
        "failure"
      ],
      "example": "success"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "description": "The API URL for the upload.",
      "example": "https://api.github.com/enterprises/300/enterprise-user-accounts-uploads/100"
    }
  },
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "uploader": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "content_type": {
      "$ref": "#/definitions/content_type"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "size": {
      "$ref": "#/definitions/size"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "sync_state": {
      "$ref": "#/definitions/sync_state"
    },
    "url": {
      "$ref": "#/definitions/url"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "content_type",
    "state",
    "size",
    "created_at",
    "updated_at",
    "sync_state",
    "url"
  ]
}
