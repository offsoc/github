<div class="SelectMenu-list" data-filterable-for="branch-filter" data-filterable-type="fuzzy">
  <% branches.each do |branch| %>
    <% selected = cache_item_filters[:branch] == branch %>
    <a
      href="<%= actions_caches_path(query: actions_filtered_query(filters: cache_item_filters, replace: { branch: !selected ? branch : nil })) %>"
      class="SelectMenu-item"
      aria-checked="<%= selected %>"
      role="menuitemradio">
      <%= primer_octicon(:check, classes: "SelectMenu-icon SelectMenu-icon--check") %>
      <%= branch %>
    </a>
  <% end %>
</div>
