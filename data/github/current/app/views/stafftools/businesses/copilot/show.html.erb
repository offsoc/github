<% page_info title: "Copilot" %>
<% if this_business.seats_plan_basic? %>
  <%= render "stafftools/businesses/copilot/seats_plan_basic",
    copilot_business: copilot_business,
    copilot_seats: copilot_seats,
    copilot_seat_emissions: copilot_seat_emissions
  %>
<% else %>
  <%= render "stafftools/businesses/copilot/seats_plan_full",
    copilot_business: copilot_business,
    seat_assignments: seat_assignments,
    active_trials: active_trials
  %>
<% end %>
