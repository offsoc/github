{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/referrer-traffic.json",
  "title": "Referrer Traffic",
  "type": "object",
  "definitions": {
    "referrer": {
      "type": "string",
      "example": "Google"
    },
    "uniques": {
      "type": "integer",
      "example": 3
    },
    "count": {
      "type": "integer",
      "example": 4
    }
  },
  "properties": {
    "referrer": {
      "$ref": "#/definitions/referrer"
    },
    "uniques": {
      "$ref": "#/definitions/uniques"
    },
    "count": {
      "$ref": "#/definitions/count"
    }
  },
  "additionalProperties": false,
  "required": [
    "referrer",
    "uniques",
    "count"
  ]
}
