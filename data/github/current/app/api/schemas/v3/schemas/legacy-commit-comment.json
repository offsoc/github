{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/legacy-commit-comment.json",
  "title": "Legacy Commit Comment",
  "type": "object",
  "definitions": {
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/comments/1"
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/commit/6dcb09b5b57875f334f61aebed695e2e4193db5e#commitcomment-1"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDEzOkNvbW1pdENvbW1lbnQx"
    },
    "position": {
      "type": [
        "number",
        "null"
      ],
      "example": 4
    },
    "line": {
      "type": [
        "number",
        "null"
      ],
      "example": 14
    },
    "path": {
      "type": [
        "string",
        "null"
      ],
      "example": "file1.txt"
    },
    "commit_id": {
      "type": "string",
      "example": "6dcb09b5b57875f334f61aebed695e2e4193db5e"
    },
    "body": {
      "type": "string",
      "example": "Great stuff"
    },
    "author_association": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-14T16:00:49Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-14T16:00:49Z"
    }
  },
  "properties": {
    "url": {
      "$ref": "#/definitions/url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "user": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "position": {
      "$ref": "#/definitions/position"
    },
    "line": {
      "$ref": "#/definitions/line"
    },
    "path": {
      "$ref": "#/definitions/path"
    },
    "commit_id": {
      "$ref": "#/definitions/commit_id"
    },
    "body": {
      "$ref": "#/definitions/body"
    },
    "author_association": {
      "$ref": "#/definitions/author_association"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "url",
    "html_url",
    "id",
    "node_id",
    "user",
    "position",
    "line",
    "path",
    "commit_id",
    "body",
    "author_association",
    "created_at",
    "updated_at"
  ]
}
