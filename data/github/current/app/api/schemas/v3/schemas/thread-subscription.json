{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/thread-subscription.json",
  "title": "Thread Subscription",
  "type": "object",
  "definitions": {
    "created_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2012-10-06T21:34:12Z"
    },
    "ignored": {
      "type": "boolean"
    },
    "reason": {
      "type": [
        "string",
        "null"
      ]
    },
    "subscribed": {
      "type": "boolean",
      "example": true
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/notifications/threads/1/subscription"
    },
    "thread_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/notifications/threads/1"
    },
    "repository_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/1"
    }
  },
  "properties": {
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "ignored": {
      "$ref": "#/definitions/ignored"
    },
    "reason": {
      "$ref": "#/definitions/reason"
    },
    "subscribed": {
      "$ref": "#/definitions/subscribed"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "thread_url": {
      "$ref": "#/definitions/thread_url"
    },
    "repository_url": {
      "$ref": "#/definitions/repository_url"
    }
  },
  "additionalProperties": false,
  "required": [
    "created_at",
    "ignored",
    "reason",
    "url",
    "subscribed"
  ]
}
