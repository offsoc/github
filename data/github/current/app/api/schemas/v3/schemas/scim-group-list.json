{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/scim-group-list.json",
  "title": "SCIM /Groups",
  "description": "SCIM /Groups provisioning endpoints",
  "type": "object",
  "definitions": {
    "schemas": {
      "description": "SCIM schema used.",
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "example": "urn:ietf:params:scim:api:messages:2.0:ListResponse"
      }
    },
    "totalResults": {
      "type": "integer",
      "example": 3
    },
    "itemsPerPage": {
      "type": "integer",
      "example": 10
    },
    "startIndex": {
      "type": "integer",
      "example": 1
    }
  },
  "properties": {
    "schemas": {
      "$ref": "#/definitions/schemas"
    },
    "totalResults": {
      "$ref": "#/definitions/totalResults"
    },
    "itemsPerPage": {
      "$ref": "#/definitions/itemsPerPage"
    },
    "startIndex": {
      "$ref": "#/definitions/startIndex"
    },
    "Resources": {
      "type": "array",
      "items": {
        "$ref": "https://schema.github.com/v3/scim-group.json#"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "schemas",
    "totalResults",
    "itemsPerPage",
    "startIndex",
    "Resources"
  ]
}
