<p><b>Double opt-in links:</b></p>
<ul class="mb-3">
  <% @opt_in_links.each do |link| %>
    <li><a href="<%= link[:url] %>"><%= link[:title] %></a></li>
  <% end %>
</ul>

<p><b>Unsubscribe links:</b></p>
<ul class="mb-3">
  <% @unsubscribe_links.each do |link| %>
    <li><a href="<%= link[:url] %>"><%= link[:title] %></a></li>
  <% end %>
</ul>
