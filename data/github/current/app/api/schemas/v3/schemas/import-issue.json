{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/import-issue.json",
  "title": "Issue",
  "description": "An issue to import",
  "type": "object",
  "definitions": {
    "timestamp": {
      "description": "A timestamp, e.g. for created_at",
      "example": "2007-10-20T05:24:19Z",
      "type": "string",
      "format": "date-time"
    },
    "issue": {
      "description": "The issue itself.",
      "type": "object",
      "properties": {
        "title": {
          "anyOf": [
            {
              "$ref": "https://schema.github.com/v3/issue.json#/definitions/title"
            },
            {
              "type": "integer"
            }
          ]
        },
        "body": {
          "$ref": "https://schema.github.com/v3/issue.json#/definitions/body"
        },
        "assignee": {
          "$ref": "https://schema.github.com/v3/issue.json#/definitions/assignee"
        },
        "milestone": {
          "$ref": "https://schema.github.com/v3/issue.json#/definitions/milestone"
        },
        "labels": {
          "$ref": "https://schema.github.com/v3/issue.json#/definitions/labels"
        },
        "closed": {
          "type": "boolean"
        },
        "updated_at": {
          "$ref": "#/definitions/timestamp"
        },
        "closed_at": {
          "$ref": "#/definitions/timestamp"
        },
        "created_at": {
          "$ref": "#/definitions/timestamp"
        }
      },
      "additionalProperties": false,
      "required": [
        "title"
      ]
    },
    "comment": {
      "description": "An issue comment to import",
      "title": "Issue Comment",
      "type": "object",
      "properties": {
        "body": {
          "$ref": "https://schema.github.com/v3/issue-comment.json#/definitions/body"
        },
        "created_at": {
          "$ref": "#/definitions/timestamp"
        }
      },
      "additionalProperties": false,
      "required": [
        "body"
      ]
    },
    "repository_id": {
      "description": "The ID of the repository.",
      "example": 42,
      "type": "integer"
    },
    "id": {
      "type": "integer"
    },
    "status": {
      "type": "string"
    },
    "errors": {
      "type": "array"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "repository_url": {
      "type": "string",
      "format": "uri"
    },
    "import_issues_url": {
      "type": "string",
      "format": "uri"
    },
    "issue_url": {
      "type": "string",
      "format": "uri"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "links": [
    {
      "href": "/repositories/{repository_id}/import/issues",
      "method": "POST",
      "rel": "create",
      "schema": {
        "properties": {
          "issue": {
            "$ref": "#/definitions/issue"
          },
          "comments": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/comment"
            }
          }
        },
        "additionalProperties": false,
        "required": [
          "issue"
        ],
        "type": "object"
      },
      "title": "Import an issue"
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "status": {
      "$ref": "#/definitions/status"
    },
    "errors": {
      "$ref": "#/definitions/errors"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "repository_url": {
      "$ref": "#/definitions/repository_url"
    },
    "import_issues_url": {
      "$ref": "#/definitions/import_issues_url"
    },
    "issue_url": {
      "$ref": "#/definitions/issue_url"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "status",
    "url",
    "repository_url",
    "import_issues_url",
    "created_at",
    "updated_at"
  ]
}
