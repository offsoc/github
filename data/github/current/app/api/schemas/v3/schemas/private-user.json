{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/private-user.json",
  "title": "Private User",
  "type": "object",
  "definitions": {
    "avatar_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/images/error/octocat_happy.gif"
    },
    "events_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/events{/privacy}"
    },
    "followers_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/followers"
    },
    "following_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/following{/other_user}"
    },
    "gists_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/gists{/gist_id}"
    },
    "gravatar_id": {
      "type": [
        "string",
        "null"
      ],
      "example": ""
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDQ6VXNlcjE="
    },
    "login": {
      "type": "string",
      "example": "octocat"
    },
    "organizations_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/orgs"
    },
    "received_events_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/received_events"
    },
    "repos_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/repos"
    },
    "site_admin": {
      "type": "boolean"
    },
    "starred_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/starred{/owner}{/repo}"
    },
    "subscriptions_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/subscriptions"
    },
    "type": {
      "type": "string",
      "example": "User"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat"
    },
    "bio": {
      "type": [
        "string",
        "null"
      ],
      "example": "There once was..."
    },
    "blog": {
      "type": [
        "string",
        "null"
      ],
      "example": "https://github.com/blog"
    },
    "twitter_username": {
      "type": [
        "string",
        "null"
      ],
      "example": "monalisa"
    },
    "company": {
      "type": [
        "string",
        "null"
      ],
      "example": "GitHub"
    },
    "email": {
      "description": "For Enterprise Managed User, if user primary email is not verified, email will contain shortcode. Otherwise returns publicly visible email address of the user.",
      "type": [
        "string",
        "null"
      ],
      "format": "email",
      "example": "octocat@github.com"
    },
    "notification_email": {
      "description": "Returns the publicly visible email address of the user.",
      "type": [
        "string",
        "null"
      ],
      "format": "email",
      "example": "octocat@github.com"
    },
    "followers": {
      "type": "integer",
      "example": 20
    },
    "following": {
      "type": "integer",
      "example": 0
    },
    "hireable": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "location": {
      "type": [
        "string",
        "null"
      ],
      "example": "San Francisco"
    },
    "name": {
      "type": [
        "string",
        "null"
      ],
      "example": "monalisa octocat"
    },
    "public_gists": {
      "type": "integer",
      "example": 1
    },
    "public_repos": {
      "type": "integer",
      "example": 2
    },
    "suspended_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2008-01-14T04:33:35Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2008-01-14T04:33:35Z"
    },
    "collaborators": {
      "type": "integer",
      "example": 8
    },
    "disk_usage": {
      "type": "integer",
      "example": 10000
    },
    "owned_private_repos": {
      "type": "integer",
      "example": 100
    },
    "business_plus": {
      "type": "boolean"
    },
    "private_gists": {
      "type": "integer",
      "example": 81
    },
    "total_private_repos": {
      "type": "integer",
      "example": 100
    },
    "ldap_dn": {
      "type": "string"
    },
    "two_factor_authentication": {
      "type": "boolean",
      "example": true
    }
  },
  "properties": {
    "avatar_url": {
      "$ref": "#/definitions/avatar_url"
