{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/runner-label.json",
  "title": "Runner Label",
  "description": "A runner label to organize self-hosted runners based on their characteristics",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 2
    },
    "name": {
      "description": "The name of the label",
      "example": "macOS",
      "type": "string"
    },
    "type": {
      "type": "string",
      "example": "custom",
      "enum": [
        "custom",
        "read-only"
      ]
    }
  },
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "type": {
      "$ref": "#/definitions/type"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "type"
  ]
}
