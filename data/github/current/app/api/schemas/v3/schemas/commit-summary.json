{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/commit-summary.json",
  "title": "Commit Summary",
  "type": "object",
  "definitions": {
    "sha": {
      "type": "string",
      "example": "7638417db6d59f3c431d3e1f261cc637155684cd"
    },
    "node_id": {
      "type": "string",
      "example": "MDY6Q29tbWl0NzYzODQxN2RiNmQ1OWYzYzQzMWQzZTFmMjYxY2M2MzcxNTU2ODRjZA=="
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/git/commits/7638417db6d59f3c431d3e1f261cc637155684cd"
    },
    "html_url": {
      "type": "string",
      "format": "uri"
    },
    "parents": {
      "type": "array",
      "example": [
        {
          "url": "https://api.github.com/repos/octocat/Hello-World/git/commits/7d1b31e74ee336d15cbd21741bc88a537ed063a0",
          "sha": "7d1b31e74ee336d15cbd21741bc88a537ed063a0"
        }
      ]
    },
    "message": {
      "type": "string",
      "example": "my commit message"
    }
  },
  "properties": {
    "sha": {
      "$ref": "#/definitions/sha"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "author": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/git-user.json#"
        }
      ]
    },
    "committer": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/git-user.json#"
        }
      ]
    },
    "parents": {
      "$ref": "#/definitions/parents"
    },
    "tree": {
      "type": "object",
      "properties": {
        "sha": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "additionalProperties": false,
      "required": [
        "sha",
        "url"
      ]
    },
    "message": {
      "$ref": "#/definitions/message"
    },
    "verification": {
      "$ref": "https://schema.github.com/v3/verification.json#"
    }
  },
  "additionalProperties": false,
  "required": [
    "sha",
    "node_id",
    "url",
    "html_url",
    "author",
    "committer",
    "parents",
    "tree",
    "message"
  ]
}
