{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/legacy-issue.json",
  "title": "Legacy Issue",
  "type": "object",
  "definitions": {
    "assignees": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "oneOf": [
          {
            "type": "null"
          },
          {
            "$ref": "https://schema.github.com/v3/simple-user.json#"
          }
        ]
      }
    },
    "body": {
      "type": [
        "string",
        "null"
      ],
      "example": "I'm having a problem with this."
    },
    "body_html": {
      "type": "string"
    },
    "body_text": {
      "type": "string"
    },
    "closed_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "comments": {
      "type": "integer",
      "example": 0
    },
    "comments_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/issues/1347/comments"
    },
    "events_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/issues/1347/events"
    },
    "timeline_url": {
      "type": "string",
      "format": "uri"
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/issues/1347"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDU6SXNzdWUx"
    },
    "labels": {
      "type": "array"
    },
    "labels_url": {
      "type": "string",
      "example": "https://api.github.com/repos/octocat/Hello-World/issues/1347/labels{/name}"
    },
    "number": {
      "type": "integer",
      "example": 1347
    },
    "repository_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World"
    },
    "state": {
      "type": "string",
      "example": "open"
    },
    "state_reason": {
      "description": "Reason for the current state",
      "example": "not_planned",
      "type": [
        "string",
        "null"
      ]
    },
    "locked": {
      "type": "boolean",
      "example": true
    },
    "active_lock_reason": {
      "type": [
        "string",
        "null"
      ],
      "example": "too heated"
    },
    "title": {
      "type": "string",
      "example": "Found a bug"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/issues/1347"
    },
    "author_association": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-22T13:33:48Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-22T13:33:48Z"
    }
  },
  "properties": {
    "assignee": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "assignees": {
      "$ref": "#/definitions/assignees"
    },
    "body": {
      "$ref": "#/definitions/body"
    },
    "body_html": {
      "$ref": "#/definitions/body_html"
    },
    "body_text": {
      "$ref": "#/definitions/body_text"
    },
    "closed_at": {
      "$ref": "#/definitions/closed_at"
    },
    "comments": {
      "$ref": "#/definitions/comments"
    },
    "comments_url": {
      "$ref": "#/definitions/comments_url"
    },
    "events_url": {
      "$ref": "#/definitions/events_url"
    },
    "timeline_url": {
      "$ref": "#/definitions/timeline_url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "labels": {
      "$ref": "#/definitions/labels"
    },
    "labels_url": {
      "$ref": "#/definitions/labels_url"
    },
    "milestone": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/milestone.json#"
        }
      ]
    },
    "number": {
      "$ref": "#/definitions/number"
    },
    "pull_request": {
      "type": "object",
      "properties": {
        "merged_at": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "diff_url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        },
        "html_url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        },
        "patch_url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        },
        "url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        }
      },
      "additionalProperties": false,
      "required": [
        "diff_url",
        "html_url",
        "patch_url",
        "url"
      ]
    },
    "repository_url": {
      "$ref": "#/definitions/repository_url"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "state_reason": {
      "$ref": "#/definitions/state_reason"
    },
    "locked": {
      "$ref": "#/definitions/locked"
    },
    "active_lock_reason": {
      "$ref": "#/definitions/active_lock_reason"
    },
    "title": {
      "$ref": "#/definitions/title"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "user": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "repository": {
      "$ref": "https://schema.github.com/v3/repository.json#"
    },
    "performed_via_github_app": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/integration.json#"
        }
      ]
    },
    "author_association": {
      "$ref": "#/definitions/author_association"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "closed_by": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "reactions": {
      "$ref": "https://schema.github.com/v3/reaction-rollup.json#"
    }
  },
  "additionalProperties": false,
  "required": [
    "assignee",
    "closed_at",
    "comments",
    "comments_url",
    "events_url",
    "html_url",
    "id",
    "node_id",
    "labels",
    "labels_url",
    "milestone",
    "number",
    "repository_url",
    "state",
    "locked",
    "title",
    "url",
    "user",
    "author_association",
    "created_at",
    "updated_at",
    "closed_by"
  ]
}
