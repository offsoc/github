proc_name = 'graphite-web'
<% if File.exist?('/etc/github/cluster') -%>
bind = ["[::]:8000"]
<% else -%>
bind = ["127.0.0.1:8000", "[::1]:8000"]
<% end -%>
workers = <%= graphite_gunicorn_workers %>
accesslog = '/opt/graphite/storage/log/webapp/access.log'
errorlog = '/opt/graphite/storage/log/webapp/error.log'
loglevel = 'info'
daemon = False
pidfile = '/opt/graphite/storage/graphite-web.pid'
timeout = 180
