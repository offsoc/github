{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/simple-user.json",
  "title": "Simple User",
  "type": "object",
  "definitions": {
    "avatar_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/images/error/octocat_happy.gif"
    },
    "events_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/events{/privacy}"
    },
    "followers_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/followers"
    },
    "following_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/following{/other_user}"
    },
    "gists_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/gists{/gist_id}"
    },
    "gravatar_id": {
      "type": [
        "string",
        "null"
      ],
      "example": ""
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDQ6VXNlcjE="
    },
    "login": {
      "type": "string",
      "example": "octocat"
    },
    "organizations_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/orgs"
    },
    "received_events_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/received_events"
    },
    "repos_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/repos"
    },
    "site_admin": {
      "type": "boolean"
    },
    "starred_url": {
      "type": "string",
      "example": "https://api.github.com/users/octocat/starred{/owner}{/repo}"
    },
    "subscriptions_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat/subscriptions"
    },
    "type": {
      "type": "string",
      "example": "User"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/users/octocat"
    }
  },
  "properties": {
    "avatar_url": {
      "$ref": "#/definitions/avatar_url"
    },
    "events_url": {
      "$ref": "#/definitions/events_url"
    },
    "followers_url": {
      "$ref": "#/definitions/followers_url"
    },
    "following_url": {
      "$ref": "#/definitions/following_url"
    },
    "gists_url": {
      "$ref": "#/definitions/gists_url"
    },
    "gravatar_id": {
      "$ref": "#/definitions/gravatar_id"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "login": {
      "$ref": "#/definitions/login"
    },
    "organizations_url": {
      "$ref": "#/definitions/organizations_url"
    },
    "received_events_url": {
      "$ref": "#/definitions/received_events_url"
    },
    "repos_url": {
      "$ref": "#/definitions/repos_url"
    },
    "site_admin": {
      "$ref": "#/definitions/site_admin"
    },
    "starred_url": {
      "$ref": "#/definitions/starred_url"
    },
    "subscriptions_url": {
      "$ref": "#/definitions/subscriptions_url"
    },
    "type": {
      "$ref": "#/definitions/type"
    },
    "url": {
      "$ref": "#/definitions/url"
    }
  },
  "additionalProperties": false,
  "required": [
    "avatar_url",
    "events_url",
    "followers_url",
    "following_url",
    "gists_url",
    "gravatar_id",
    "html_url",
    "id",
    "node_id",
    "login",
    "organizations_url",
    "received_events_url",
    "repos_url",
    "site_admin",
    "starred_url",
    "subscriptions_url",
    "type",
    "url"
  ]
}
