      "$ref": "#/definitions/id"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "type": {
      "$ref": "#/definitions/type"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "active": {
      "$ref": "#/definitions/active"
    },
    "events": {
      "$ref": "#/definitions/events"
    },
    "config": {
