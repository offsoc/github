<%= content_tag(:p, @preamble, class: "f4-mktg color-fg-muted mb-3") if @preamble.present? %>

<%= content_tag(:h2, @title, class: "h5-mktg mb-2") if @title.present? %>

<%= content_tag(:ul, class: list_classes) do %>
  <% document_links.each do |document_link| %>
  <%= render document_link %>
  <% end %>
<% end %>

<%= content_tag(:p, @postamble, class: "f4-mktg color-fg-muted") if @postamble.present? %>
