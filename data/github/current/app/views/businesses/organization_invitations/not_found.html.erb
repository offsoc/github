<%= render Primer::Beta::Blankslate.new(narrow: true, mt: 5) do |c| %>
  <% c.with_visual_icon(icon: "alert") %>

  <% c.with_heading(tag: :h2) do %>
    Invitation not found
  <% end %>

  <% c.with_description do %>
    We’ve looked all over but couldn’t find your invitation.
  <% end %>

  <% c.with_primary_action(href: enterprise_path(this_business)) do %>
    <%= "Go to the #{this_business.name} profile page" %>
  <% end %>
<% end %>
