import {screen} from '@testing-library/react'
import {render} from '@github-ui/react-core/test-utils'
import { {{~routeComponent~}} } from '../routes/{{routeComponent}}'
import {get{{routeComponent}}RoutePayload} from '../test-utils/mock-data'

test('Renders the {{routeComponent}}', () => {
  const routePayload = get{{routeComponent}}RoutePayload()
  render(<{{routeComponent}} />, {
    routePayload,
  })
  expect(screen.getByRole('article')).toHaveTextContent(routePayload.someField)
})
