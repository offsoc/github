<% page_info title: "Pages" %>

<%= render layout: "settings/settings" do %>
  <%= render Primer::Beta::Subhead.new(mb: 4) do |component| %>
    <% component.with_heading(tag: :h2, id: "pages_label") do %>
      Pages
    <% end %>
  <% end %>

  <%= render partial: "settings/pages_protected_domains/protected_domains", locals: {
    domains: pages_protected_domains,
    owner: current_user,
    new_domain_path: new_settings_pages_protected_domain_path,
  } %>
<% end %>
