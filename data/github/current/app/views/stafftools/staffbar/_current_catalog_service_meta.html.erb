<% if employee? && GitHub.staffbar_service_info_enabled? && controller.respond_to?(:logical_service) %>
  <meta name="current-catalog-service" content="<%= controller.logical_service %>" data-turbo-transient>
  <meta name="current-catalog-service-url" content="https://catalog.githubapp.com/services/<%= controller.logical_service %>/support" data-turbo-transient>
<% end %>

<% if controller.respond_to?(:logical_service_hash) %>
  <meta name="current-catalog-service-hash" content="<%= controller.logical_service_hash %>">
<% end %>
