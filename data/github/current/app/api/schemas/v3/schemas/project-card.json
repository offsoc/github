{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/project-card.json",
  "title": "Project Card",
  "description": "Project cards represent a scope of work.",
  "type": "object",
  "definitions": {
    "id": {
      "description": "The project card's ID",
      "example": 42,
      "type": "integer"
    },
    "card_id": {
      "description": "The ID of the card.",
      "type": "integer"
    },
    "archived": {
      "description": "Whether or not the card is archived",
      "example": false,
      "type": "boolean"
    },
    "content_id": {
      "description": "The unique identifier of the content associated with the card",
      "example": 42,
      "type": "integer"
    },
    "content_type": {
      "description": "The piece of content associated with the card",
      "example": "PullRequest",
      "type": "string"
    },
    "note": {
      "description": "The project card's note",
      "example": "Update all gems",
      "type": "string"
    },
    "legacy_note": {
      "description": "The project card's note",
      "example": "Update all gems",
      "type": [
        "string",
        "null"
      ]
    },
    "column_id": {
      "description": "The unique identifier of the column the card should be moved to",
      "example": 42,
      "type": "integer"
    },
    "project_id": {
      "description": "The ID of the project.",
      "type": "integer"
    },
    "node_id": {
      "type": "string",
      "example": "MDExOlByb2plY3RDYXJkMTQ3OA=="
    },
    "project_card_note": {
      "type": [
        "string",
        "null"
      ],
      "example": "Add payload for delete Project column"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/projects/columns/cards/1478"
    },
    "column_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/projects/columns/367"
    },
    "project_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/projects/120"
    },
    "content_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/api-playground/projects-test/issues/3"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2016-09-05T14:21:06Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2016-09-05T14:20:22Z"
    }
  },
  "links": [
    {
      "href": "/projects/columns/{column_id}/cards",
      "method": "POST",
      "rel": "create-in-column-legacy",
      "schema": {
        "properties": {
          "note": {
            "$ref": "#/definitions/legacy_note"
          },
          "content_id": {
            "$ref": "#/definitions/content_id"
          },
          "content_type": {
            "$ref": "#/definitions/content_type"
          }
        },
        "dependencies": {
          "content_type": [
            "content_id"
          ]
        },
        "type": [
          "object",
          "null"
        ]
      },
      "title": "Create a new project card in a column"
    },
    {
      "href": "/projects/columns/cards/{card_id}",
      "method": "PATCH",
      "rel": "update-legacy",
      "schema": {
        "properties": {
          "note": {
            "$ref": "#/definitions/legacy_note"
          },
          "archived": {
            "$ref": "#/definitions/archived"
          }
        },
        "type": [
          "object",
          "null"
        ]
      },
      "title": "Update an existing project card"
    },
    {
      "href": "/projects/{project_id}/cards",
      "method": "POST",
      "rel": "create-legacy",
      "schema": {
        "properties": {
          "note": {
            "$ref": "#/definitions/legacy_note"
          },
          "content_id": {
            "$ref": "#/definitions/content_id"
          },
          "content_type": {
            "$ref": "#/definitions/content_type"
          }
        },
        "dependencies": {
          "content_type": [
            "content_id"
          ]
        },
        "type": [
          "object",
          "null"
        ]
      },
      "title": "Create"
    },
    {
      "title": "Create a new project card in a column",
      "method": "POST",
      "href": "/projects/columns/{column_id}/cards",
      "rel": "create-in-column",
      "schema": {
        "oneOf": [
          {
            "properties": {
              "content_type": {
                "$ref": "#/definitions/content_type"
              },
              "content_id": {
                "$ref": "#/definitions/content_id"
              }
            },
            "dependencies": {
              "content_type": [
                "content_id"
              ]
            },
            "additionalProperties": false
          },
          {
            "properties": {
              "note": {
                "$ref": "#/definitions/note"
              }
            },
            "additionalProperties": false
          }
        ],
        "type": "object"
      }
    },
    {
      "title": "Create a project card",
      "method": "POST",
      "href": "/projects/{project_id}/cards",
      "rel": "create",
      "schema": {
        "properties": {
          "note": {
            "$ref": "#/definitions/note"
          },
          "content_id": {
            "$ref": "#/definitions/content_id"
          },
          "content_type": {
            "$ref": "#/definitions/content_type"
          }
        },
        "dependencies": {
          "content_type": [
            "content_id"
          ]
        },
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    },
    {
      "title": "Update an existing project card",
      "method": "PATCH",
      "href": "/projects/columns/cards/{card_id}",
      "rel": "update",
      "schema": {
        "properties": {
          "note": {
            "$ref": "#/definitions/note"
          },
          "archived": {
            "$ref": "#/definitions/archived"
          }
        },
        "type": [
          "object",
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Delete a project card",
      "method": "DELETE",
      "rel": "delete",
      "href": "/projects/columns/cards/{card_id}",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "note": {
      "$ref": "#/definitions/project_card_note"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "column_url": {
      "$ref": "#/definitions/column_url"
    },
    "project_url": {
      "$ref": "#/definitions/project_url"
    },
    "content_url": {
      "$ref": "#/definitions/content_url"
    },
    "creator": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "archived": {
      "$ref": "#/definitions/archived"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "note",
    "url",
    "column_url",
    "project_url",
    "creator",
    "created_at",
    "updated_at"
  ]
}
