  <%= render Primer::Beta::Subhead.new(hide_border: true, mx: 5, mb: 3) do |component| %>
    <% component.with_heading(tag: :h2) do %>
      Your sponsors
    <% end %>
    <% component.with_actions do %>
      <%= render(Sponsors::Dashboard::YourSponsors::ExportSponsorshipsComponent.new(
        sponsorable: sponsorable,
      )) %>
    <% end %>
  <% end %>

  <%= render Primer::Alpha::TabNav.new(label: "Sponsors Navigation") do |c| %>
    <% c.with_tab(
        selected: selected_tab == :active_sponsors,
        href: sponsorable_dashboard_your_sponsors_path(sponsorable),
        ml: 5) do %>
      Active sponsors
    <% end %>
    <% c.with_tab(
        selected: selected_tab == :activity_feed,
        href: sponsorable_dashboard_activities_path(sponsorable)) do %>
      Activity feed
    <% end %>
  <% end %>
