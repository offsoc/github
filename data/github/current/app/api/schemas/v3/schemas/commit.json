          "$ref": "#/definitions/url"
        },
        "author": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "https://schema.github.com/v3/git-user.json#"
            }
          ]
        },
        "committer": {
          "oneOf": [
            {
              "type": "null"
            },
