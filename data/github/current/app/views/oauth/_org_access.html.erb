<%# view is an instance of Oauth::AuthorizeView %>
<%= render partial: "oauth/blocked", collection: view.blocked_organizations %>
<%= render partial: "oauth/allowed", collection: view.allowed_organizations %>
<%= render partial: "oauth/request", collection: view.requestable_organizations, locals: { view: view } %>
<%= render partial: "oauth/grant", collection: view.grantable_organizations, locals: { view: view } %>
<%= render partial: "oauth/denied", collection: view.denied_organizations %>
