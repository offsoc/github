# Metrics collection for dependency-graph-api
<% if dependency_graph_enabled? %>
<Plugin processes>
  ProcessMatch "dependency-graph-api" "puma.*app"
  ProcessMatch "dependency-graph-api-aqueduct-worker" "config\/resqued_aqueduct.rb"
  ProcessMatch "dependency-graph-api-processor-manifest-file-changed-worker" "script\/etl\/repo_manifest_file_changes"
  ProcessMatch "dependency-graph-api-processor-manifest-file-deleted-worker.hcl" "script\/etl\/repo_manifest_file_deleted"
</Plugin>
<%- else -%>
# File intentionally blank.
# This feature of GitHub Enterprise Server is not enabled.
<%- end -%>
