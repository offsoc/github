<% if current_permission == active_permission %>
  <%= render Primer::Beta::Label.new(scheme: :success, float: :right, title: "Label: Active") do %>
    Active
  <% end %>
<% elsif Ability::ACTION_RANKING[current_permission] < Ability::ACTION_RANKING[active_permission] %>
  <%= render Primer::Beta::Label.new(scheme: :primary, float: :right, title: "Label: Inactive") do %>
    Inactive
  <% end %>
<% end %>
