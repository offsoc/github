{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/commit-status.json",
  "title": "Commit Status",
  "type": "object",
  "definitions": {
    "description": {
      "type": [
        "string",
        "null"
      ],
      "example": "Build has completed successfully"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDY6U3RhdHVzMQ=="
    },
    "state": {
      "type": "string",
      "example": "success"
    },
    "context": {
      "type": "string",
      "example": "continuous-integration/jenkins"
    },
    "target_url": {
      "type": "string",
      "format": "uri",
      "example": "https://ci.example.com/1000/output"
    },
    "required": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "avatar_url": {
      "type": [
        "string",
        "null"
      ],
      "format": "uri",
      "example": "https://github.com/images/error/hubot_happy.gif"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/statuses/6dcb09b5b57875f334f61aebed695e2e4193db5e"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2012-07-20T01:19:13Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2012-07-20T01:19:13Z"
    }
  },
  "properties": {
    "description": {
      "$ref": "#/definitions/description"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "context": {
      "$ref": "#/definitions/context"
    },
    "target_url": {
      "$ref": "#/definitions/target_url"
    },
    "required": {
      "$ref": "#/definitions/required"
    },
    "avatar_url": {
      "$ref": "#/definitions/avatar_url"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "creator": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "description",
    "id",
    "node_id",
    "state",
    "context",
    "target_url",
    "avatar_url",
    "url",
    "created_at",
    "updated_at",
    "creator"
  ]
}
