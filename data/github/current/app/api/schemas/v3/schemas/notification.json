{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/notification.json",
  "title": "Notification",
  "type": "object",
  "definitions": {
    "id": {
      "type": "string",
      "example": "1"
    },
    "reason": {
      "type": "string",
      "example": "subscribed"
    },
    "unread": {
      "type": "boolean",
      "example": true
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2014-11-07T22:01:45Z"
    },
    "last_read_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2014-11-07T22:01:45Z"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/notifications/threads/1"
    },
    "subscription_url": {
      "type": "string",
      "format": "uri"
    }
  },
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "repository": {
      "$ref": "https://schema.github.com/v3/minimal-repository.json#"
    },
    "subject": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "latest_comment_url": {
          "type": "string",
          "format": "uri"
        },
        "type": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "title",
        "url",
        "latest_comment_url",
        "type"
      ]
    },
    "reason": {
      "$ref": "#/definitions/reason"
    },
    "unread": {
      "$ref": "#/definitions/unread"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "last_read_at": {
      "$ref": "#/definitions/last_read_at"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "subscription_url": {
      "$ref": "#/definitions/subscription_url"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "repository",
    "subject",
    "reason",
    "unread",
    "updated_at",
    "url",
    "subscription_url"
  ]
}
