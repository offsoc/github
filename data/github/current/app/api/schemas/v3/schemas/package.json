{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/package.json",
  "title": "Package",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "name": {
      "type": "string",
      "example": "super-linter"
    },
    "package_type": {
      "type": "string",
      "example": "docker",
      "enum": [
        "npm",
        "maven",
        "rubygems",
        "docker",
        "nuget",
        "container"
      ]
    },
    "url": {
      "type": "string",
      "example": "https://api.github.com/orgs/github/packages/container/super-linter"
    },
    "html_url": {
      "type": "string",
      "example": "https://github.com/orgs/github/packages/container/package/super-linter"
    },
    "version_count": {
      "type": "integer",
      "example": 1
    },
    "visibility": {
      "type": "string",
      "example": "private",
      "enum": [
        "private",
        "public",
        "internal"
      ]
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-04-10T20:09:31Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2014-03-03T18:58:10Z"
    },
    "token": {
      "description": "Unique token optionally used to restore a package.",
      "type": "string"
    }
  },
  "links": [
    {
      "title": "Delete Current User Package",
      "method": "DELETE",
      "href": "/user/packages/{package_type}/{package}",
      "rel": "delete-current-user",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Delete User Package",
      "method": "DELETE",
      "href": "/user/{user_id}/packages/{package_type}/{package}",
      "rel": "delete-user",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Delete Organization Package",
      "method": "DELETE",
      "href": "/organizations/{organization_id}/packages/{package_type}/{package}",
      "rel": "delete-org",
      "schema": {
        "type": [
          "null"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Restore Current User Package",
      "method": "POST",
      "href": "/user/packages/{package_type}/{package}/restore",
      "rel": "restore-current-user",
      "schema": {
        "properties": {
          "token": {
            "$ref": "#/definitions/token"
          }
        },
        "type": [
          "null",
          "object"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Restore User Package",
      "method": "POST",
      "href": "/user/{user_id}/packages/{package_type}/{package}/restore",
      "rel": "restore-user",
      "schema": {
        "properties": {
          "token": {
            "$ref": "#/definitions/token"
          }
        },
        "type": [
          "null",
          "object"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Restore Organization Package",
      "method": "POST",
      "href": "/organizations/{organization_id}/packages/{package_type}/{package}/restore",
      "rel": "restore-org",
      "schema": {
        "properties": {
          "token": {
            "$ref": "#/definitions/token"
          }
        },
        "type": [
          "null",
          "object"
        ],
        "additionalProperties": false
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "package_type": {
      "$ref": "#/definitions/package_type"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "owner": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "repository": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/minimal-repository.json#"
        }
      ]
    },
    "version_count": {
      "$ref": "#/definitions/version_count"
    },
    "visibility": {
      "$ref": "#/definitions/visibility"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "package_type",
    "url",
    "html_url",
    "visibility",
    "version_count",
    "created_at",
    "updated_at"
  ]
}
