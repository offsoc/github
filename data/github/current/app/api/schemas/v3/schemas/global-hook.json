{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/global-hook.json",
  "title": "Global Hook",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/admin/hooks/1"
    },
    "type": {
      "type": "string",
      "example": "Global"
    },
    "name": {
      "type": "string",
      "example": "web"
    },
    "active": {
      "type": "boolean",
      "example": true
    },
    "events": {
      "type": "array",
      "example": [
        "organization",
        "user"
      ]
    },
    "ping_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/admin/hooks/1/pings"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2017-12-07T00:14:59Z"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2017-12-07T00:14:59Z"
    }
  },
  "properties": {
