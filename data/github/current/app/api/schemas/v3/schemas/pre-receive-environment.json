    },
    "default_environment": {
      "$ref": "#/definitions/default_environment"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "hooks_count": {
      "$ref": "#/definitions/hooks_count"
    },
    "download": {
      "$ref": "https://schema.github.com/v3/pre-receive-environment-download.json#/definitions/pre-receive-environment-download-subschema"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
