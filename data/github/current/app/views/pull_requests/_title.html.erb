<%# rubocop:disable Lint/InstanceVariable %>
<%= render :partial => 'comments/title', :locals => {
  :pull         => @pull,
  :comment      => @pull.issue,
  :issue        => @pull.issue,
  :title        => @pull.title.blank? ? "Untitled" : @pull.title,
  :sticky       => local_assigns[:sticky].present? ? sticky : false
} %>
