{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/marketplace-listing-subscription-item.json",
  "title": "Marketplace Listing Subscription Item",
  "type": "object",
  "definitions": {
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/orgs/github"
    },
    "id": {
      "type": "integer",
      "example": 4
    },
    "type": {
      "type": "string",
      "example": "Organization"
    },
    "login": {
      "type": "string",
      "example": "github"
    },
    "email": {
      "type": [
        "string",
        "null"
      ],
      "format": "email"
    },
    "organization_billing_email": {
      "type": [
        "string",
        "null"
      ],
      "format": "email",
      "example": "billing@github.com"
    }
  },
  "properties": {
    "url": {
      "$ref": "#/definitions/url"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "type": {
      "$ref": "#/definitions/type"
    },
    "login": {
      "$ref": "#/definitions/login"
    },
    "email": {
      "$ref": "#/definitions/email"
    },
    "organization_billing_email": {
      "$ref": "#/definitions/organization_billing_email"
    },
    "marketplace_purchase": {
      "$ref": "https://schema.github.com/v3/marketplace-purchase.json#"
    },
    "marketplace_pending_change": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/marketplace-pending-change.json#"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "url",
    "id",
    "type",
    "login",
    "marketplace_purchase"
  ]
}
