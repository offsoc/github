{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/marketplace-listing-plan.json",
  "title": "Marketplace Listing Plan",
  "type": "object",
  "definitions": {
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/marketplace_listing/plans/1313"
    },
    "accounts_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/marketplace_listing/plans/1313/accounts"
    },
    "id": {
      "type": "integer",
      "example": 1313
    },
    "number": {
      "type": "integer",
      "example": 3
    },
    "name": {
      "type": "string",
      "example": "Pro"
    },
    "description": {
      "type": "string",
      "example": "A professional-grade CI solution"
    },
    "has_free_trial": {
      "type": "boolean",
      "example": true
    },
    "price_model": {
      "type": "string",
      "example": "flat-rate"
    },
    "unit_name": {
      "type": [
        "string",
        "null"
      ]
    },
    "monthly_price_in_cents": {
      "type": "integer",
      "example": 1099
    },
    "state": {
      "type": "string",
      "example": "published"
    },
    "yearly_price_in_cents": {
      "type": "integer",
      "example": 11870
    },
    "bullets": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "example": [
        "Up to 25 private repositories",
        "11 concurrent builds"
      ]
    }
  },
  "properties": {
    "url": {
      "$ref": "#/definitions/url"
    },
    "accounts_url": {
      "$ref": "#/definitions/accounts_url"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "number": {
      "$ref": "#/definitions/number"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "description": {
      "$ref": "#/definitions/description"
    },
    "has_free_trial": {
      "$ref": "#/definitions/has_free_trial"
    },
    "price_model": {
      "$ref": "#/definitions/price_model"
    },
    "unit_name": {
      "$ref": "#/definitions/unit_name"
    },
    "monthly_price_in_cents": {
      "$ref": "#/definitions/monthly_price_in_cents"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "yearly_price_in_cents": {
      "$ref": "#/definitions/yearly_price_in_cents"
    },
    "bullets": {
      "$ref": "#/definitions/bullets"
    }
  },
  "additionalProperties": false,
  "required": [
    "url",
    "accounts_url",
    "id",
    "number",
    "name",
    "description",
    "has_free_trial",
    "price_model",
    "unit_name",
    "monthly_price_in_cents",
    "state",
    "yearly_price_in_cents",
    "bullets"
  ]
}
