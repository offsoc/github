<% @key_file = "/data/user/common/authorized_keys" if @key_file.nil? -%>
<% if @secrets["admin-ssh-pubkey"] && !@secrets["admin-ssh-pubkey"].empty? -%>
<% admin_pubkey = "no-agent-forwarding,no-port-forwarding,no-user-rc,no-X11-forwarding " + @secrets["admin-ssh-pubkey"] -%>
<% if File.foreach(@key_file).grep(admin_pubkey + "\n").none? -%>
<%= admin_pubkey || "" %>
<% end -%>
<% end -%>
<%= File.read(@key_file) -%>