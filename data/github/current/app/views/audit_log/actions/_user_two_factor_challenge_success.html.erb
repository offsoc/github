<% if view.entry.hit.data[:two_factor_type] %>
  <% if view.entry.hit.data[:two_factor_type] == "passwordless" %>
    Two-factor auth requirement met by passkey <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "github_mobile" %>
    Two-factor auth challenge solved with GitHub Mobile <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "otp" %>
    Two-factor auth challenge solved with one time password <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "app" %>
    Two-factor auth challenge solved with authenticator app <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "sms" %>
    Two-factor auth challenge solved with SMS <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "u2f" %>
    Two-factor auth challenge solved with security key <%= view.entry.hit.data[:note].try(:downcase) %>
  <% elsif view.entry.hit.data[:two_factor_type] == "passkey" %>
    Two-factor auth challenge solved with passkey <%= view.entry.hit.data[:note].try(:downcase) %>
  <% end %>
<% else %>
  Two-factor auth challenge solved <%= view.entry.hit.data[:note].try(:downcase) %>
<% end %>
