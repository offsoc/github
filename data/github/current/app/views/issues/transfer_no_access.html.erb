<%= render layout: "issues/issues" do %>
  <%= render Primer::Beta::Blankslate.new do |c| %>
    <% c.with_heading(tag: :h2) do %>
      <%= "This issue has been moved to a repository you don't have access to" + (local_assigns[:issue_deleted] ? " or has been deleted since being transferred" : "") + "." %>
    <% end %>
  <% end %>
<% end %>
