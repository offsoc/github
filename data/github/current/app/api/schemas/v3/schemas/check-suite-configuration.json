{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/check-suite-configuration.json",
  "title": "Check Suite Configuration",
  "type": "object",
  "properties": {
    "preferences": {
      "type": "object",
      "properties": {
        "auto_trigger_checks": {
          "type": "array",
          "items": {
            "$ref": "https://schema.github.com/v3/check-suite-app-preference.json#"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "auto_trigger_checks"
      ]
    },
    "repository": {
      "$ref": "https://schema.github.com/v3/minimal-repository.json#"
    }
  },
  "additionalProperties": false,
  "required": [
    "repository"
  ]
}
