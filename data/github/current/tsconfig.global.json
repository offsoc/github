{
  "extends": "@github-ui/tsconfig/base.json",
  "include": ["app/**/*.ts", "app/**/*.tsx", "ui/**/*.ts", "ui/**/*.tsx", "ui/**/.storybook/*.ts"],
  "exclude": [
    "app/assets/workers",
    "test/**/*.ts",
    "ui/packages/memex",
    "ui/packages/mock-fetch",
    "ui/packages/analytics-test-utils",
    "ui/packages/service-worker",
    "ui/packages/socket-worker"
  ],
  "references": [
    {
      "path": "ui/packages/memex/tsconfig.composite.json"
    }
  ],
  "compilerOptions": {
    "strictPropertyInitialization": false
  }
}
