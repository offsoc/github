<span data-target="async-audit-log.spinner" hidden>
  <include-fragment class="m-4 p-4 d-flex flex-column flex-items-center">
    <div data-hide-on-error class="m-4 p-4 d-flex flex-column flex-items-center">
      <%= render(Primer::Beta::Spinner.new) %>
      <div class="color-fg-muted no-wrap">Loading audit log entries&hellip;</div>
    </div>
    <div data-show-on-error hidden>
      Something went wrong.
      <%= render(Primer::Beta::Button.new("data-retry-button": "", scheme: :default, size: :small)) { "Retry" } %>
    </div>
  </include-fragment>
</span>


