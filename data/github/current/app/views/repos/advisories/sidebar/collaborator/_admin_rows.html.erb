<% view.admin_users.each do |admin_user| %>
  <div class="my-2">
    <%= render partial: "repos/advisories/sidebar/collaborator", locals: { view: view, member: admin_user, show_removal: false } %>
  </div>
<% end %>

<% view.admin_teams.each do |admin_team| %>
  <div class="my-2">
    <%= render :partial => "repos/advisories/sidebar/team_collaborator", locals: { member: admin_team, show_removal: false } %>
  </div>
<% end %>
