{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/integration-installation-request.json",
  "title": "GitHub app installation request",
  "type": "object",
  "definitions": {
    "id": {
      "description": "Unique identifier of the request",
      "example": 37,
      "type": "integer"
    },
    "node_id": {
      "type": "string",
      "example": "MDExOkludGVncmF0aW9uMQ=="
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "account": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        },
        {
          "$ref": "https://schema.github.com/v3/enterprise.json#"
        }
      ]
    },
    "requester": {
      "$ref": "https://schema.github.com/v3/simple-user.json#"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "account",
    "requester",
    "created_at"
  ]
}
