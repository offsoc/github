<% if @has_search_filter %>
  <%= render Memex::ProjectList::Blankslate::BlankslateSearchFilterComponent.new(
    member_or_current_user: member_or_current_user?,
    parsed_query: @parsed_query,
    org_project: org_project?,
  ) %>
<% elsif @is_org_projects_disabled %>
  <%= render Memex::ProjectList::Blankslate::BlankslateOrgProjectsDisabledComponent.new(owner: @owner) %>
<% elsif linker? %>
  <%= render Memex::ProjectList::Blankslate::BlankslateLinkerComponent.new(
    parsed_query: @parsed_query,
    org_project: org_project?,
    current_user_can_push: current_user_can_push?,
    team_member: team_member?,
  ) %>
<% elsif org? || user? || projects_dashboard_context? %>
  <%= render Memex::ProjectList::Blankslate::BlankslateOrgUserComponent.new(
    has_no_projects: @has_no_projects,
    member_or_current_user: member_or_current_user?,
    parsed_query: @parsed_query,
    is_org_empty: @is_org_empty,
    is_user_empty: @is_user_empty,
    owner: @owner
  ) %>
<% end %>
