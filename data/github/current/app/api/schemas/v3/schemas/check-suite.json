{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/check-suite.json",
  "title": "CheckSuite",
  "description": "A suite of checks performed on the code of a given code change",
  "type": "object",
  "definitions": {
    "head_sha": {
      "description": "The SHA of the head commit that is being checked.",
      "example": "009b8a3a9ccbb128af87f9b1c0f4c62e8a304f6d",
      "type": "string"
    },
    "owner": {
      "description": "The owner of the repository.",
      "type": "string"
    },
    "repo": {
      "description": "The name of the repository.",
      "type": "string"
    },
    "check_suite_id": {
      "description": "The ID of the check suite.",
      "type": "integer"
    },
    "repository_id": {
      "description": "The ID of the repository.",
      "type": "integer",
      "example": 42
    },
    "id": {
      "type": "integer",
      "example": 5
    },
    "node_id": {
      "type": "string",
      "example": "MDEwOkNoZWNrU3VpdGU1"
    },
    "head_branch": {
      "type": [
        "string",
        "null"
      ],
      "example": "master"
    },
    "status": {
      "type": [
        "string",
        "null"
      ],
      "example": "completed"
    },
    "conclusion": {
      "type": [
        "string",
        "null"
      ],
      "example": "neutral"
    },
    "url": {
      "type": [
        "string",
        "null"
      ],
      "example": "https://api.github.com/repos/github/hello-world/check-suites/5"
    },
    "before": {
      "type": [
        "string",
        "null"
      ],
      "example": "146e867f55c26428e5f9fade55a9bbf5e95a7912"
    },
    "after": {
      "type": [
        "string",
        "null"
      ],
      "example": "d6fde92930d4715a2b49857d24b940956b26d2d3"
    },
    "created_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "updated_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "latest_check_runs_count": {
      "type": "integer"
    },
    "check_runs_url": {
      "type": "string"
    },
    "pull_requests": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "https://schema.github.com/v3/pull-request-minimal.json#"
      }
    },
    "rerequestable": {
      "type": "boolean"
    },
    "runs_rerequestable": {
      "type": "boolean"
    }
  },
  "links": [
    {
      "title": "Create a new check suite",
      "method": "POST",
      "rel": "create-legacy",
      "href": "/repositories/{repository_id}/check-suites",
      "schema": {
        "properties": {
          "head_sha": {
            "$ref": "#/definitions/head_sha"
          }
        },
        "type": "object",
        "required": [
          "head_sha"
        ]
      }
    },
    {
      "title": "Request check suites",
      "method": "POST",
      "rel": "request",
      "href": "/repositories/{repository_id}/check-suite-requests",
      "schema": {
        "properties": {
          "head_sha": {
            "$ref": "#/definitions/head_sha"
          }
        },
        "type": "object",
        "required": [
          "head_sha"
        ]
      }
    },
    {
      "title": "Create a new check suite",
      "method": "POST",
      "href": "/repositories/{repository_id}/check-suites",
      "rel": "create",
      "schema": {
        "properties": {
          "head_sha": {
            "$ref": "#/definitions/head_sha"
          }
        },
        "type": "object",
        "required": [
          "head_sha"
        ],
        "additionalProperties": false
      }
    },
    {
      "title": "Rerequest check suite",
      "method": "POST",
      "rel": "rerequest",
      "href": "/repositories/{repository_id}/check-suites/{check_suite_id}/rerequest",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "only_failed_checks": {
            "type": "boolean",
            "optional": true
          }
        }
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "head_branch": {
      "$ref": "#/definitions/head_branch"
    },
    "status": {
      "$ref": "#/definitions/status"
    },
    "conclusion": {
      "$ref": "#/definitions/conclusion"
    },
    "head_sha": {
      "$ref": "#/definitions/head_sha"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "before": {
      "$ref": "#/definitions/before"
    },
    "after": {
      "$ref": "#/definitions/after"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "app": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/integration.json#"
        }
      ]
    },
    "head_commit": {
      "$ref": "https://schema.github.com/v3/simple-commit.json#"
    },
    "repository": {
      "$ref": "https://schema.github.com/v3/minimal-repository.json#"
    },
    "latest_check_runs_count": {
      "$ref": "#/definitions/latest_check_runs_count"
    },
    "check_runs_url": {
      "$ref": "#/definitions/check_runs_url"
    },
    "pull_requests": {
      "$ref": "#/definitions/pull_requests"
    },
    "rerequestable": {
      "$ref": "#/definitions/rerequestable"
    },
    "runs_rerequestable": {
      "$ref": "#/definitions/runs_rerequestable"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "head_branch",
    "status",
    "conclusion",
    "head_sha",
    "url",
    "before",
    "after",
    "created_at",
    "updated_at",
    "app",
    "head_commit",
    "repository",
    "latest_check_runs_count",
    "check_runs_url",
    "pull_requests"
  ]
}
