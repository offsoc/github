<%= render(SlashCommands::DialogComponent.new(breadcrumbs: breadcrumbs)) do %>
  <ul role="listbox" class="js-slash-command-menu-items">
    <% items.each do |item| %>
      <%= render SlashCommands::MenuItemComponent.new(
        id: item.id,
        title: item.text,
        description: item.description,
        url: path,
        hidden_fields: hidden_fields(item.value)
      ) %>
    <% end %>
  </ul>
<% end %>
