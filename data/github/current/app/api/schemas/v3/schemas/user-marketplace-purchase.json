{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/user-marketplace-purchase.json",
  "title": "User Marketplace Purchase",
  "type": "object",
  "definitions": {
    "billing_cycle": {
      "type": "string",
      "example": "monthly"
    },
    "next_billing_date": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2017-11-11T00:00:00Z"
    },
    "unit_count": {
      "type": [
        "number",
        "null"
      ]
    },
    "updated_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2017-11-02T01:12:12Z"
    },
    "on_free_trial": {
      "type": "boolean",
      "example": true
    },
    "free_trial_ends_on": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2017-11-11T00:00:00Z"
    }
  },
  "properties": {
    "billing_cycle": {
      "$ref": "#/definitions/billing_cycle"
    },
    "next_billing_date": {
      "$ref": "#/definitions/next_billing_date"
    },
    "unit_count": {
      "$ref": "#/definitions/unit_count"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "on_free_trial": {
      "$ref": "#/definitions/on_free_trial"
    },
    "free_trial_ends_on": {
      "$ref": "#/definitions/free_trial_ends_on"
    },
    "account": {
      "$ref": "https://schema.github.com/v3/marketplace-account.json#"
    },
    "plan": {
      "$ref": "https://schema.github.com/v3/marketplace-listing-plan.json#"
    }
  },
  "additionalProperties": false,
  "required": [
    "billing_cycle",
    "next_billing_date",
    "unit_count",
    "updated_at",
    "on_free_trial",
    "free_trial_ends_on",
    "account",
    "plan"
  ]
}
