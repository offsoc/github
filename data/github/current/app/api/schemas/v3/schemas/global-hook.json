      "required": [

      ]
    },
    "ping_url": {
      "$ref": "#/definitions/ping_url"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    }
  },
