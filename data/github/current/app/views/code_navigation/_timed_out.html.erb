<%# erblint:counter DeprecatedComponentsCounter 1 %>
<div class="flex-auto d-flex flex-justify-between f6 px-3 py-1 text-small color-bg-subtle js-code-nav-retry" data-code-nav-kind="<%= nav_kind %>" data-code-nav-url="<%= nav_url %>">
  <%= render Primer::BlankslateComponent.new(
    icon: "search",
    icon_size: :small,
    title: "Your code navigation lookup timed out.",
    title_tag: :h4,
  ) do %>
    <%= render(Primer::Beta::Button.new(scheme: :link, font_weight: :normal, data: code_navigation_hydro_click_tracking("retry_after_timeout", repo: current_repository, ref: tree_name, language: language, backend: "", code_nav_context: code_nav_context, retry_backend: ""))) { "Retry your lookup" } -%>.
  <% end %>
</div>
