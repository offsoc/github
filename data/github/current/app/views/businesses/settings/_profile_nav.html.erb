<% selected = local_assigns.fetch(:selected, "") %>
<%= render(Primer::Alpha::UnderlineNav.new(label: "Default with nav element")) do |component| %>
  <% component.with_tab(href: settings_profile_enterprise_path(this_business), selected: selected == "profile") { "Enterprise profile" } %>
  <% component.with_tab(href: settings_profile_footer_enterprise_path(this_business), selected: selected == "footer") { "Custom footer" } %>
<% end %>
