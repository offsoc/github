{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/reaction.json",
  "title": "Reaction",
  "description": "Reactions to conversations provide a way to help people express their feelings more simply and effectively.",
  "type": "object",
  "definitions": {
    "content": {
      "description": "The reaction to use",
      "example": "heart",
      "type": "string",
      "enum": [
        "+1",
        "-1",
        "laugh",
        "confused",
        "heart",
        "hooray",
        "rocket",
        "eyes"
      ]
    },
    "reaction_id": {
      "description": "The ID of the reaction.",
      "type": "integer"
    },
    "repository_id": {
      "description": "The ID of the repository.",
      "example": 42,
      "type": "integer"
    },
    "issue_number": {
      "description": "The number of the issue.",
      "type": "integer"
    },
    "comment_id": {
      "description": "The ID of the comment.",
      "type": "integer"
    },
    "team_id": {
      "description": "The ID of the team.",
      "type": "integer"
    },
    "discussion_number": {
      "description": "The number of the discussion.",
      "type": "integer"
    },
    "comment_number": {
      "description": "The number of the comment.",
      "type": "integer"
    },
    "release_id": {
      "description": "The ID of the release.",
      "type": "integer"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDg6UmVhY3Rpb24x"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2016-05-20T20:09:31Z"
    }
  },
