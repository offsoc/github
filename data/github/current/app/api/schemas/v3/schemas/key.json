{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/key.json",
  "title": "Key",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "created_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2014-12-10T15:53:42Z"
    },
    "title": {
      "type": "string",
      "example": "octocat@octomac"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/user/keys/1"
    },
    "verified": {
      "type": "boolean",
      "example": true
    },
    "read_only": {
      "type": "boolean",
      "example": true
    },
    "key": {
      "type": "string",
      "properties": {
        "key": {
          "$ref": "#/definitions/key"
        },
        "id": {
          "$ref": "#/definitions/id"
        },
        "created_at": {
          "$ref": "#/definitions/created_at"
        },
        "title": {
          "$ref": "#/definitions/title"
        },
        "url": {
          "$ref": "#/definitions/url"
        },
        "verified": {
          "$ref": "#/definitions/verified"
        },
        "read_only": {
          "$ref": "#/definitions/read_only"
        }
      },
      "additionalProperties": false,
      "required": [
        "key",
        "id",
        "created_at",
        "title",
        "url",
        "verified",
        "read_only"
      ],
      "example": "ssh-rsa AAA..."
    }
  },
  "properties": {
    "key": {
      "$ref": "#/definitions/key"
    },
    "id": {
      "$ref": "#/definitions/id"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "title": {
      "$ref": "#/definitions/title"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "verified": {
      "$ref": "#/definitions/verified"
    },
    "read_only": {
      "$ref": "#/definitions/read_only"
    }
  },
  "additionalProperties": false,
  "required": [
    "key",
    "id",
    "created_at",
    "title",
    "url",
    "verified",
    "read_only"
  ]
}
