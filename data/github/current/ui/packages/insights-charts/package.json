{
  "name": "@github-ui/insights-charts",
  "description": "Insights Charts web components",
  "version": "1.6.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --dev",
    "build:primer": "node script/generate-primer-files.js && tsc src/primer/*.ts --declaration --emitDeclarationOnly && eslint --fix src/primer",
    "clean": "rm -rf dist",
    "dev": "rollup -cw & nodemon ./sandbox/sandbox.cjs",
    "lint": "eslint --cache --max-warnings=0 . --ext .js,.ts,.tsx,.json",
    "prebuild": "npm run clean && npm run lint",
    "start": "node ./sandbox/sandbox.cjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.0",
    "@babel/plugin-proposal-decorators": "7.23.0",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.23.0",
    "@github-ui/eslintrc": "*",
    "@github-ui/jest": "*",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-multi-entry": "6.0.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.5.0",
    "@types/dompurify": "2.4.0",
    "express": "4.20.0",
    "jest-canvas-mock": "2.5.0",
    "jest-sinon": "1.1.0",
    "nodemon": "3.0.1",
    "rollup": "3.29.5",
    "rollup-plugin-analyzer": "4.0.0",
    "sinon": "13.0.1",
    "typescript": "*"
  },
  "dependencies": {
    "@kurkle/color": "0.3.2",
    "@primer/primitives": "*",
    "chart.js": "3.7.0",
    "chartjs-adapter-date-fns": "3.0.0",
    "date-fns": "3.3.1",
    "dompurify": "3.1.3",
    "ml-regression-simple-linear": "2.0.3"
  }
}
