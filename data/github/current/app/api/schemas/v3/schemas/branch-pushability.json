{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/branch-pushability.json",
  "title": "Branch pushability",
  "type": "object",
  "definitions": {
    "name": {
      "type": "string",
      "example": "master"
    }
  },
  "properties": {
    "name": {
      "$ref": "#/definitions/name"
    },
    "commit": {
      "type": "object",
      "properties": {
        "sha": {
          "type": [
            "string",
            "null"
          ]
        },
        "url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        }
      },
      "additionalProperties": false
    },
    "protected": {
      "type": "boolean"
    },
    "pattern": {
      "type": [
        "string",
        "null"
      ]
    },
    "required_signatures": {
      "type": "boolean"
    },
    "required_status_checks": {
      "type": "array",
      "example": [
        "continuous-integration/travis-ci"
      ]
    },
    "required_approving_review_count": {
      "type": "integer"
    },
    "required_linear_history": {
      "type": "boolean"
    },
    "allow_actor": {
      "type": "boolean"
    },
    "allow_deletions": {
      "type": "boolean"
    },
    "allow_force_pushes": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "commit",
    "pattern",
    "required_signatures",
    "required_status_checks",
    "required_approving_review_count",
    "required_linear_history",
    "allow_actor",
    "allow_deletions",
    "allow_force_pushes"
  ]
}
