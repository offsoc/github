<section class="js-render-needs-enrichment render-needs-enrichment position-relative"
  data-identity="<%= identity %>"
  data-host="<%= uri_origin(iframe_url) %>"
  data-src="<%= iframe_url %>"
  data-type="<%= render_type %>"
  aria-label="<%= render_type %> rendered output container"
>
  <div class="js-render-enrichment-target" data-json="<%= viewdata_as_json %>" data-plain="<%= view_data %>">
    <div class="render-plaintext-hidden">
      <pre lang="<%= render_type %>" aria-label="Raw <%= render_type %> code"><%= view_data %></pre>
    </div>
  </div>
  <span class="js-render-enrichment-loader d-flex flex-justify-center flex-items-center width-full" style="min-height:100px" role="presentation">
    <%= render(Primer::Beta::Spinner.new(size: :small, classes: "octospinner", mx: :auto)) %>
  </span>
</section>
