{
  "name": "@github-ui/memex",
  "description": "Memex frontend code",
  "version": "1.61.0",
  "private": true,
  "main": "src/client/index.tsx",
  "prettier": "@github/prettier-config",
  "scripts": {
    "build:relay": "cd ../../../ && npm run compile-relay",
    "serve:dev": "node ./dist/server-dev.js",
    "start": "npm run build:relay && NODE_ENV=development APP_ENV=development ENABLE_PROFILING=TRUE IS_STANDALONE=TRUE TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack serve --config webpack.config.ts",
    "start:vnc": "script/vnc-server",
    "stop:vnc": "docker stop setup-display-display-1 setup-display-novnc-1",
    "build:staging": "npm run build:relay && mkdir -p dist/memex-stories && cp src/server-dev.js dist/server-dev.js && ENABLE_PROFILING=TRUE USE_HOT_RELOADING=FALSE IS_STANDALONE=TRUE APP_ENV=staging TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config webpack.config.ts && cp -r dev-environment/. dist/memex-stories && cp dist/memex-stories/index.html dist/memex-stories/404.html",
    "test": "npm run build:relay && TZ=utc jest --forceExit --detectOpenHandles",
    "test:playwright": "npx playwright test --config=src/playwright-tests/playwright.config.ts",
    "test:playwright:debug": "DEBUG_TESTS=1 DEBUG_SLOMO_SPEED=2000 DISPLAY=127.0.0.1:0 npm run test:playwright -- --project=chromium",
    "test:playwright:chromium": "npm run test:playwright -- --project=chromium",
    "test:playwright:firefox": "npm run test:playwright -- --project=firefox",
    "test:playwright:webkit": "npm run test:playwright -- --project=webkit",
    "lint": "npm run eslint && npm run lint:dependencies && npm run lint:feature-flags",
    "lint:fix": "npm run eslint:fix && npm run prettier",
    "lint:feature-flags": "node script/extract-enabled-features.mjs",
    "eslint": "eslint \"src/**/*.+(js|jsx|ts|tsx)\" \"script/**/*.+(js|ts|mjs)\" \"webpack.config.ts\"",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:dependencies": "depcruise --config ./.dependency-cruiser.js src/client/ src/tests/ src/playwright-tests/ --output-type err-long",
    "prettier": "prettier --write \"src/**/*.+(js|jsx|ts|tsx)\" \"**/*.md\" \"**/*.json\" \"**/*.js\" \"**/*.html\"",
    "prettier:ci": "prettier --check \"src/**/*.+(js|jsx|ts|tsx)\" \"**/*.json\" \"**/*.js\" \"**/*.html\"",
    "check": "tsc --noEmit",
    "check:playwright": "tsc --noEmit -p src/playwright-tests/",
    "check:tests": "tsc --noEmit -p src/tests/",
    "check:scripts": "tsc --noEmit -p tsconfig.webpack.json",
    "check:all": "npm run check && npm run check:tests && npm run check:playwright",
    "validate:state-providers": "node script/validate-state-providers.mjs",
    "upload-test-results": "node script/upload-test-results.js",
    "download-images": "ts-node -P tsconfig.webpack.json  script/download-user-images.ts",
    "tsc": "tsc"
  },
  "url": "https://github.com/github/memex/issues",
  "comments": {
    "test:ci": "Actions have a limited number of cores per cpu, so runInBand improves speed by limiting the start up/waiting time that attempting to parallelize these tasks causes. We also run the tests in silent mode, since any logging during teardown will cause an error code > 0, leading to a false failure"
  },
