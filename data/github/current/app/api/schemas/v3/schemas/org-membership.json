{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/org-membership.json",
  "title": "Org Membership",
  "type": "object",
  "definitions": {
    "state": {
      "type": "string",
      "example": "active"
    },
    "role": {
      "type": "string",
      "example": "admin"
    },
    "organization_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/orgs/octocat"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/orgs/octocat/memberships/defunkt"
    }
  },
  "properties": {
    "state": {
      "$ref": "#/definitions/state"
    },
    "role": {
      "$ref": "#/definitions/role"
    },
    "organization_url": {
      "$ref": "#/definitions/organization_url"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "organization": {
      "$ref": "https://schema.github.com/v3/organization-simple.json#"
    },
    "user": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "permissions": {
      "type": "object",
      "properties": {
        "can_create_repository": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "required": [
        "can_create_repository"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "state",
    "role",
    "organization_url",
    "url",
    "organization",
    "user"
  ]
}
