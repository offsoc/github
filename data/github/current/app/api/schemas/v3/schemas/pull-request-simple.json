{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/pull-request-simple.json",
  "title": "Pull Request Simple",
  "type": "object",
  "definitions": {
    "assignees": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "oneOf": [
          {
            "type": "null"
          },
          {
            "$ref": "https://schema.github.com/v3/simple-user.json#"
          }
        ]
      }
    },
    "requested_reviewers": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "oneOf": [
          {
            "type": "null"
          },
          {
            "$ref": "https://schema.github.com/v3/simple-user.json#"
          }
        ]
      }
    },
    "requested_teams": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "https://schema.github.com/v3/team-simple.json#"
      }
    },
    "labels": {
      "type": "array"
    },
    "draft": {
      "description": "Indicates whether or not the pull request is a draft.",
      "example": false,
      "type": "boolean"
    },
    "body": {
      "type": [
        "string",
        "null"
      ],
      "example": "Please pull these awesome changes"
    },
    "closed_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2011-01-26T19:01:12Z"
    },
    "comments_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/issues/1347/comments"
    },
    "commits_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/pulls/1347/commits"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-01-26T19:01:12Z"
    },
    "diff_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/pull/1347.diff"
    },
    "html_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/pull/1347"
    },
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDExOlB1bGxSZXF1ZXN0MQ=="
    },
    "issue_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/issues/1347"
    },
    "merge_commit_sha": {
      "type": [
        "string",
        "null"
      ],
      "example": "e5bd3914e2e596debea16f433f57875b5b90bcd6"
    },
    "merged_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2011-01-26T19:01:12Z"
    },
    "number": {
      "type": "integer",
      "example": 1347
    },
    "patch_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/pull/1347.patch"
    },
    "review_comment_url": {
      "type": "string",
      "example": "https://api.github.com/repos/octocat/Hello-World/pulls/comments{/number}"
    },
    "review_comments_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/pulls/1347/comments"
    },
    "statuses_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/statuses/6dcb09b5b57875f334f61aebed695e2e4193db5e"
    },
    "state": {
      "type": "string",
      "example": "open"
    },
    "locked": {
      "type": "boolean",
      "example": true
    },
    "active_lock_reason": {
      "type": [
        "string",
        "null"
      ],
      "example": "too heated"
    },
    "title": {
      "type": "string",
      "example": "new-feature"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "example": "2011-01-26T19:01:12Z"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/pulls/1347"
    },
    "author_association": {
      "type": "string",
      "example": "OWNER"
    }
