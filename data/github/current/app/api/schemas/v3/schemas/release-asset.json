{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/release-asset.json",
  "title": "Release Asset",
  "description": "Data related to a release.",
  "type": "object",
  "definitions": {
    "name": {
      "description": "The file name of the asset.",
      "type": "string",
      "example": "Team Environment"
    },
    "label": {
      "description": "An short description of the asset.",
      "type": "string",
      "example": "bug"
    },
    "state": {
      "description": "State of the release asset.",
      "type": "string",
      "enum": [
        "uploaded"
      ],
      "example": "open"
    },
    "owner": {
      "description": "The owner of the repository.",
      "type": "string"
    },
    "repo": {
      "description": "The name of the repository.",
      "type": "string"
    },
    "asset_id": {
      "description": "The ID of the asset.",
      "type": "integer"
    },
    "repository_id": {
      "description": "The ID of the repository.",
      "type": "integer",
      "example": 42
    },
    "id": {
      "type": "integer"
    },
    "content_type": {
      "type": "string"
    },
    "size": {
      "type": "integer"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "node_id": {
      "type": "string"
    },
    "download_count": {
      "type": "integer"
    },
    "release_asset_label": {
      "type": [
        "string",
        "null"
      ]
    },
    "browser_download_url": {
      "type": "string",
      "format": "uri"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "links": [
    {
      "title": "Edit a release asset",
      "method": "PATCH",
      "rel": "update",
      "href": "/repositories/{repository_id}/releases/assets/{asset_id}",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "label": {
            "$ref": "#/definitions/label"
          },
          "state": {
            "$ref": "#/definitions/state"
          }
        },
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    },
    {
      "title": "Edit a release asset",
      "method": "POST",
      "rel": "update",
      "href": "/repositories/{repository_id}/releases/assets/{asset_id}",
      "schema": {
        "properties": {
          "name": {
            "$ref": "#/definitions/name"
          },
          "label": {
            "$ref": "#/definitions/label"
          },
          "state": {
            "$ref": "#/definitions/state"
          }
        },
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    },
    {
      "title": "Delete a release asset",
      "method": "DELETE",
      "rel": "delete",
      "href": "/repositories/{repository_id}/releases/assets/{asset_id}",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "content_type": {
      "$ref": "#/definitions/content_type"
    },
    "size": {
      "$ref": "#/definitions/size"
    },
    "state": {
      "$ref": "#/definitions/state"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "download_count": {
      "$ref": "#/definitions/download_count"
    },
    "label": {
      "$ref": "#/definitions/release_asset_label"
    },
    "uploader": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "browser_download_url": {
      "$ref": "#/definitions/browser_download_url"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "content_type",
    "size",
    "state",
    "url",
    "node_id",
    "download_count",
    "label",
    "uploader",
    "browser_download_url",
    "created_at",
    "updated_at"
  ]
}
