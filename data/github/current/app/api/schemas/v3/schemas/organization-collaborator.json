{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/organization-collaborator.json",
  "title": "Organization Collaborator",
  "description": "An outside collaborator in an organization.",
  "type": "object",
  "definitions": {
    "org": {
      "description": "The name of the organization.",
      "type": "string"
    },
    "username": {
      "description": "The username of the outside_collaborator.",
      "type": "string",
      "example": "username1"
    },
    "organization_id": {
      "description": "The ID of the organization.",
      "type": "integer"
    }
  },
  "links": [
    {
      "title": "Remove outside collaborator",
      "method": "DELETE",
      "rel": "delete",
      "href": "/organizations/{organization_id}/outside_collaborators/{username}",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    }
  ]
}
