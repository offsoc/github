<%= form_tag conduit_register_disinterest_path do %>
  <%= hidden_field_tag :event_id, item.event_id, id: "disinterest-event-id-#{item.event_id}" %>
  <%= hidden_field_tag :resource_id, item.resource_id, id: "disinterest-resource-id-#{item.event_id}" %>
  <%= hidden_field_tag :resource_type, item.resource_type, id: "disinterest-resource-type-#{item.event_id}" %>
  <%= hidden_field_tag :card_type, item.event_type, id: "disinterest-card-type-#{item.event_id}" %>
  <%= hidden_field_tag :undo, false, id: "disinterest-undo-false-#{item.event_id}" %>
  <%= hidden_field_tag :identifier, item.identifier, id: "disinterest-identifier-id-#{item.event_id}" %>
  <%= hidden_field_tag :reason_dismissed, :dismissed %>
  <%= body %>
<% end %>
