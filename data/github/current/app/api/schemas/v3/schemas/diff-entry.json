{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/diff-entry.json",
  "title": "Diff Entry",
  "type": "object",
  "definitions": {
    "additions": {
      "type": "integer",
      "example": 103
    },
    "blob_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/blob/6dcb09b5b57875f334f61aebed695e2e4193db5e/file1.txt"
    },
    "changes": {
      "type": "integer",
      "example": 124
    },
    "contents_url": {
      "type": "string",
      "format": "uri",
      "example": "https://api.github.com/repos/octocat/Hello-World/contents/file1.txt?ref=6dcb09b5b57875f334f61aebed695e2e4193db5e"
    },
    "deletions": {
      "type": "integer",
      "example": 21
    },
    "filename": {
      "type": "string",
      "example": "file1.txt"
    },
    "raw_url": {
      "type": "string",
      "format": "uri",
      "example": "https://github.com/octocat/Hello-World/raw/6dcb09b5b57875f334f61aebed695e2e4193db5e/file1.txt"
    },
    "sha": {
      "type": "string",
      "example": "bbcd538c8e72b8c175046e27cc8f907076331401"
    },
    "status": {
      "type": "string",
      "example": "added"
    },
    "patch": {
      "type": "string",
      "example": "@@ -132,7 +132,7 @@ module Test @@ -1000,7 +1000,7 @@ module Test"
    },
    "previous_filename": {
      "type": "string",
      "example": "file.txt"
    }
  },
  "properties": {
    "additions": {
      "$ref": "#/definitions/additions"
    },
    "blob_url": {
      "$ref": "#/definitions/blob_url"
    },
    "changes": {
      "$ref": "#/definitions/changes"
    },
    "contents_url": {
      "$ref": "#/definitions/contents_url"
    },
    "deletions": {
      "$ref": "#/definitions/deletions"
    },
    "filename": {
      "$ref": "#/definitions/filename"
    },
    "raw_url": {
      "$ref": "#/definitions/raw_url"
    },
    "sha": {
      "$ref": "#/definitions/sha"
    },
    "status": {
      "$ref": "#/definitions/status"
    },
    "patch": {
      "$ref": "#/definitions/patch"
    },
    "previous_filename": {
      "$ref": "#/definitions/previous_filename"
    }
  },
  "additionalProperties": false,
  "required": [
    "additions",
    "blob_url",
    "changes",
    "contents_url",
    "deletions",
    "filename",
    "raw_url",
    "sha",
    "status"
  ]
