# frozen_string_literal: true

source "https://rubygems.org"

# Specify your gem's dependencies in github-scout.gemspec
gemspec :name => "github-scout.nightly-ci"
gem 'github-linguist', path: '../linguist'
gem 'rake'
gem 'bundler', '>= 1.10'
gem 'minitest', '~> 5.14'
gem 'rake-compiler', '~> 0.9'
gem 'mocha'
group :debug do
  gem 'byebug' if RUBY_VERSION >= '2.2'
end
