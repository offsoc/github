<%= render(Primer::Beta::Subhead.new(hide_border: @hide_border)) do |c| %>
  <% c.with_heading(tag: :h2) { @heading } %>
  <% c.with_description.with_content(@subheading) if @subheading.present? %>

  <% if feedback_link? || phase_label? %>
    <% c.with_actions do %>
      <%= render(feedback_link) if feedback_link? %>
      <%= render(phase_label) if phase_label? %>
    <% end %>
  <% end %>
<% end %>
