<% view.unique_collaborating_users.each do |user| %>
  <div class="my-2">
    <%= render :partial => "repos/advisories/sidebar/collaborator", locals: { member: user, view: view, show_removal: true } %>
  </div>
<% end %>

<% view.unique_collaborating_teams.each do |team| %>
  <div class="my-2">
    <%= render :partial => "repos/advisories/sidebar/team_collaborator", locals: { member: team, view: view, show_removal: true } %>
  </div>
<% end %>
