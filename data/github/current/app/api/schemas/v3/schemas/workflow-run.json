{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "https://schema.github.com/v3/workflow-run.json",
  "title": "WorkflowRun",
  "description": "An invocation of a workflow",
  "type": "object",
  "definitions": {
    "head_sha": {
      "description": "The SHA of the head commit that points to the version of the worflow being run.",
      "example": "009b8a3a9ccbb128af87f9b1c0f4c62e8a304f6d",
      "type": "string"
    },
    "repo": {
      "description": "The name of the repository.",
      "type": "string",
      "example": "hub"
    },
    "repository_id": {
      "description": "The ID of the repository.",
      "type": "integer",
      "example": 42
    },
    "id": {
      "type": "integer",
      "description": "The ID of the workflow run.",
      "example": 5
    },
    "name": {
      "type": [
        "string",
        "null"
      ],
      "description": "The name of the workflow run.",
      "example": "Build"
    },
    "run_number": {
      "type": "integer",
      "description": "The auto incrementing run number for the workflow run.",
      "example": 106
    },
    "run_attempt": {
      "type": "integer",
      "description": "Attempt number of the run, 1 for first attempt and higher if the workflow was re-run.",
      "example": 1
    },
    "node_id": {
      "type": "string",
      "example": "MDEwOkNoZWNrU3VpdGU1"
    },
    "check_suite_id": {
      "type": "integer",
      "description": "The ID of the associated check suite.",
      "example": 42
    },
    "check_suite_node_id": {
      "type": "string",
      "description": "The node ID of the associated check suite.",
      "example": "MDEwOkNoZWNrU3VpdGU0Mg=="
    },
    "head_branch": {
      "type": [
        "string",
        "null"
      ],
      "example": "master"
    },
    "head_repository_id": {
      "type": "integer",
      "example": 5
    },
    "event": {
      "type": "string",
      "example": "push"
    },
    "status": {
      "type": [
        "string",
        "null"
      ],
      "example": "completed"
    },
    "conclusion": {
      "type": [
        "string",
        "null"
      ],
      "example": "neutral"
    },
    "workflow_id": {
      "type": "integer",
      "description": "The ID of the parent workflow.",
      "example": 5
    },
    "url": {
      "type": "string",
      "description": "The URL to the workflow run.",
      "example": "https://api.github.com/repos/github/hello-world/actions/runs/5"
    },
    "html_url": {
      "type": [
        "string"
      ],
      "example": "https://github.com/github/hello-world/suites/4"
    },
    "created_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "updated_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "run_started_at": {
      "description": "The start time of the latest run. Resets on re-run.",
      "type": "string",
      "format": "date-time"
    },
    "cancel_url": {
      "description": "The URL to cancel the workflow run.",
      "type": "string",
      "example": "https://api.github.com/repos/github/hello-world/actions/runs/5/cancel"
    },
    "rerun_url": {
      "description": "The URL to rerun the workflow run.",
      "type": "string",
      "example": "https://api.github.com/repos/github/hello-world/actions/runs/5/rerun"
    },
    "previous_attempt_url": {
      "description": "The URL to the previous attempted run of this workflow, if one exists.",
      "type": [
        "string",
        "null"
      ],
      "example": "https://api.github.com/repos/github/hello-world/actions/runs/5/attempts/3"
    },
    "workflow_url": {
      "description": "The URL to the workflow.",
      "type": "string",
      "example": "https://api.github.com/repos/github/hello-world/actions/workflows/main.yml"
    },
    "artifacts_url": {
      "description": "The URL to the artifacts for the workflow run.",
      "type": "string",
      "example": "https://api.github.com/repos/github/hello-world/actions/runs/5/rerun/artifacts"
    },
    "jobs_url": {
      "description": "The URL to the jobs for the workflow run.",
      "type": "string",
      "example": "https://api.github.com/repos/github/hello-world/actions/runs/5/jobs"
    },
    "logs_url": {
      "description": "The URL to download the logs for the workflow run.",
      "type": "string",
      "example": "https://api.github.com/repos/github/hello-world/actions/runs/5/logs"
    },
    "check_suite_url": {
      "description": "The URL to the associated check suite.",
      "type": "string",
      "example": "https://api.github.com/repos/github/hello-world/check-suites/12"
    },
    "pull_requests": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "https://schema.github.com/v3/pull-request-minimal.json#"
      }
    },
    "run_id": {
      "type": "integer",
      "description": "The ID of the workflow run.",
      "example": 5
    }
  },
  "links": [
    {
      "title": "Retries a workflow",
      "href": "/repositories/{repository_id}/actions/runs/{run_id}/retry",
      "method": "POST",
      "rel": "retry",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    },
    {
      "title": "Cancels a workflow run",
      "href": "/repositories/{repository_id}/actions/runs/{run_id}/cancel",
      "method": "POST",
      "rel": "cancel",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    },
    {
      "title": "Force cancels a workflow run",
      "href": "/repositories/{repository_id}/actions/runs/{run_id}/force-cancel",
      "method": "POST",
      "rel": "force-cancel",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    },
    {
      "title": "Deletes the logs of a workflow run",
      "href": "/repositories/{repository_id}/actions/runs/{run_id}/logs",
      "method": "DELETE",
      "rel": "delete-logs",
      "schema": {
        "additionalProperties": false,
        "type": [
          "object",
          "null"
        ]
      }
    }
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "name": {
      "$ref": "#/definitions/name"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "check_suite_id": {
      "$ref": "#/definitions/check_suite_id"
    },
    "check_suite_node_id": {
      "$ref": "#/definitions/check_suite_node_id"
    },
    "run_number": {
      "$ref": "#/definitions/run_number"
    },
    "run_attempt": {
      "$ref": "#/definitions/run_attempt"
    },
    "head_branch": {
      "$ref": "#/definitions/head_branch"
    },
    "head_repository_id": {
      "$ref": "#/definitions/head_repository_id"
    },
    "event": {
      "$ref": "#/definitions/event"
    },
    "status": {
      "$ref": "#/definitions/status"
    },
    "conclusion": {
      "$ref": "#/definitions/conclusion"
    },
    "head_sha": {
      "$ref": "#/definitions/head_sha"
    },
    "workflow_id": {
      "$ref": "#/definitions/workflow_id"
    },
    "url": {
      "$ref": "#/definitions/url"
    },
    "html_url": {
      "$ref": "#/definitions/html_url"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "updated_at": {
      "$ref": "#/definitions/updated_at"
    },
    "run_started_at": {
      "$ref": "#/definitions/run_started_at"
    },
    "head_commit": {
      "$ref": "https://schema.github.com/v3/simple-commit.json#"
    },
    "repository": {
      "$ref": "https://schema.github.com/v3/minimal-repository.json#"
    },
    "head_repository": {
      "$ref": "https://schema.github.com/v3/minimal-repository.json#"
    },
    "cancel_url": {
      "$ref": "#/definitions/cancel_url"
    },
    "rerun_url": {
      "$ref": "#/definitions/rerun_url"
    },
    "previous_attempt_url": {
      "$ref": "#/definitions/previous_attempt_url"
    },
    "workflow_url": {
      "$ref": "#/definitions/workflow_url"
    },
    "artifacts_url": {
      "$ref": "#/definitions/artifacts_url"
    },
    "jobs_url": {
      "$ref": "#/definitions/jobs_url"
    },
    "logs_url": {
      "$ref": "#/definitions/logs_url"
    },
    "check_suite_url": {
      "$ref": "#/definitions/check_suite_url"
    },
    "pull_requests": {
      "$ref": "#/definitions/pull_requests"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "head_branch",
    "run_number",
    "event",
    "status",
    "conclusion",
    "head_sha",
    "workflow_id",
    "url",
    "html_url",
    "created_at",
    "updated_at",
    "head_commit",
    "head_repository",
    "repository",
    "jobs_url",
    "logs_url",
    "check_suite_url",
    "cancel_url",
    "rerun_url",
    "artifacts_url",
    "workflow_url",
    "pull_requests"
  ]
}
