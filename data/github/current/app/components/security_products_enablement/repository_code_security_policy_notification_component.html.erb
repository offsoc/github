<%= render Primer::Box.new(test_selector: TEST_SELECTOR, **system_arguments) do %>
  <%= render Primer::Beta::Octicon.new(:"shield-lock", color: :muted) %>
  <%= render Primer::Beta::Text.new(color: :muted, font_size: :small) do %>
    Managed by
    <% if organization? %>
      <%= render(Primer::Beta::Link.new(href: organization_path(blocked_by), classes: "Link--inTextBlock")) { blocked_by.safe_profile_name } %>
    <% else %>
      <%= render(Primer::Beta::Link.new(href: enterprise_path(blocked_by), classes: "Link--inTextBlock")) { blocked_by.name } %>
    <% end %>
  <% end %>
<% end %>
