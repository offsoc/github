<%# `view` is an instance of Marketplace::Listings::OnboardingPageView %>

<%= render layout: "marketplace_listings/admin_layout", locals: { marketplace_listing: marketplace_listing, selected_tab: :overview } do %>
  <%= render Primer::Beta::Subhead.new do |component| %>
    <% component.with_heading(tag: :h2) do %>Publish your app to Marketplace<% end %>
  <% end %>

  <%= render "marketplace_listings/completion_statuses", view: view, agreement: agreement, listing: marketplace_listing %>
  <% if marketplace_listing.can_delist? && GitHub.flipper[:allow_admin_to_delist_listing].enabled?(current_user) %>
      <%= render "marketplace_listing_admins/delist_app", view: view, agreement: agreement, listing: marketplace_listing %>
  <% end %>
<% end %>
