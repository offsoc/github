<dl>
  <dt>Name</dt>
  <dd><%= view.name %></dd>
</dl>
<dl>
  <dt>Created</dt>
  <dd><%= view.created_at %></dd>
</dl>
<dl>
  <dt>Last update</dt>
  <dd><%= view.updated_at %></dd>
</dl>
<dl>
  <dt>Owner</dt>
  <dd>
    <% if view.user %>
      <%= link_to view.gist.user_param, stafftools_user_path(view.gist.user_param) %>
    <% else %>
      <%= view.user_param %>
    <% end %>
  </dd>
</dl>
<dl>
  <dt>Visibility</dt>
  <dd>
    <%= view.gist.visibility %>
  </dd>
</dl>
<dl>
  <dt>Last push</dt>
  <dd>
    <%= view.last_push %>
  </dd>
</dl>
<dl>
  <dt>Last maintenance</dt>
  <dd>
    <%= view.last_maintenance_at %>
    (<%= view.last_maintenance_status %>)
  </dd>
</dl>
<% unless GitHub.enterprise? %>
  <dl>
    <dt>Exceptions</dt>
    <dd>
      <%= link_to "Search Sentry", view.sentry_link %>
    </dd>
  </dl>
<% end %>
