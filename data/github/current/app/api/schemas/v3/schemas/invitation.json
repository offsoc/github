{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/invitation.json",
  "title": "Invitation",
  "type": "object",
  "definitions": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "node_id": {
      "type": "string"
    },
    "login": {
      "type": [
        "string",
        "null"
      ],
      "example": "monalisa"
    },
    "email": {
      "type": [
        "string",
        "null"
      ],
      "example": "octocat@github.com"
    },
    "role": {
      "type": "string",
      "example": "direct_member"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "example": "2016-11-30T06:46:10-08:00"
    },
    "failed_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "example": "2016-11-30T06:46:10-08:00"
    },
    "failed_reason": {
      "type": [
        "string",
        "null"
      ],
      "example": "Invitation expired. User did not accept this invite for 7 days."
    },
    "team_count": {
      "type": "integer",
      "example": 2
    },
    "invitation_teams_url": {
      "type": "string"
    }
  },
  "properties": {
    "id": {
      "$ref": "#/definitions/id"
    },
    "node_id": {
      "$ref": "#/definitions/node_id"
    },
    "login": {
      "$ref": "#/definitions/login"
    },
    "email": {
      "$ref": "#/definitions/email"
    },
    "role": {
      "$ref": "#/definitions/role"
    },
    "created_at": {
      "$ref": "#/definitions/created_at"
    },
    "failed_at": {
      "$ref": "#/definitions/failed_at"
    },
    "failed_reason": {
      "$ref": "#/definitions/failed_reason"
    },
    "inviter": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "https://schema.github.com/v3/simple-user.json#"
        }
      ]
    },
    "team_count": {
      "$ref": "#/definitions/team_count"
    },
    "invitation_teams_url": {
      "$ref": "#/definitions/invitation_teams_url"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "node_id",
    "login",
    "email",
    "role",
    "created_at",
    "inviter"
  ]
}
