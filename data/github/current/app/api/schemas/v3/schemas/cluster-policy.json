{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.github.com/v3/cluster-policy.json",
  "title": "Cluster Policy",
  "type": "object",
  "definitions": {
    "upload_url": {
      "type": "string",
      "format": "uri"
    },
    "asset_upload_url": {
      "type": "string",
      "format": "uri"
    },
    "same_origin": {
      "type": "boolean"
    }
  },
  "properties": {
    "upload_url": {
      "$ref": "#/definitions/upload_url"
    },
    "asset_upload_url": {
      "$ref": "#/definitions/asset_upload_url"
    },
    "same_origin": {
      "$ref": "#/definitions/same_origin"
    },
    "header": {
      "type": "object",
      "properties": {
        "Accept": {
          "type": "string"
        },
        "Authorization": {
          "type": "string"
        },
        "GitHub-Remote-Auth": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [

      ]
    },
    "form": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "size": {
          "type": "number"
        },
        "content_type": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "migration_id": {
          "type": "number"
        },
        "business_id": {
          "type": "number",
          "description": "ID of the business for which a cluster policy is being created.",
          "example": 12345
        },
        "enterprise_installation_id": {
          "type": "number",
          "description": "ID of the Enterprise Server installation for which a cluster policy is being created.",
          "example": 12345
        }
      },
      "additionalProperties": false,
      "required": [
        "name",
        "size",
        "content_type"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "upload_url",
    "same_origin",
    "header",
    "form"
  ]
}
