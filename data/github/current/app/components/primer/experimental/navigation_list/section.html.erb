<% if heading %>
  <%= heading %>
<% end %>
<%= render(Primer::BaseComponent.new(tag: :li, classes: "ActionList-item ActionList-item--hasSubItem")) do %>
  <%= render(Primer::ConditionalWrapper.new(condition: @lazy_load_section, tag: "lazy-load-section")) do %>
    <%= render(Primer::BaseComponent.new(tag: :ul, **@system_arguments)) do %>
      <% items.each do |item| %>
        <%= item %>
      <% end %>
      <% if @lazy_load_section %>
        <%= show_more_item %>
      <% end %>
    <% end %>
  <% end %>
<% end %>

